static void
init(void)
{
	/* from input.c: */
	/* we will never free this */
	basepf.next_to_pgetc = basepf.buf = xmalloc((1 ? 1024 : 1024));

	/* from trap.c: */
	signal(17, ((__sighandler_t) 0));
	/* bash re-enables SIGHUP which is SIG_IGNed on entry.
	 * Try: "trap '' HUP; bash; echo RET" and type "kill -HUP $$"
	 */
	signal(1, ((__sighandler_t) 0));

	/* from var.c: */
	{
		char **envp;
		const char *p;
		struct stat st1, st2;

		initvar();
		for (envp = environ; envp && *envp; envp++) {
			if (strchr(*envp, '=')) {
				setvareq(*envp, 0x01|0x08);
			}
		}

		setvar("PPID", utoa(getppid()), 0);

		p = lookupvar("PWD");
		if (p)
			if (*p != '/' || stat(p, &st1) || stat(".", &st2)
			 || st1.st_dev != st2.st_dev || st1.st_ino != st2.st_ino)
				p = '\0';
		setpwd(p, 0);
	}
}

/*
 * Process the shell command line arguments.
 */
static void
procargs(char **argv)
{
	int i;
	const char *xminusc;
	char **xargv;

	xargv = argv;
	((*ash_ptr_to_globals_misc).arg0 ) = xargv[0];
	/* if (xargv[0]) - mmm, this is always true! */
		xargv++;
	for (i = 0; i < NOPTS; i++)
		((*ash_ptr_to_globals_misc).optlist )[i] = 2;
	argptr = xargv;
	if (options(1)) {
		/* it already printed err message */
		raise_exception(1);
	}
	xargv = argptr;
	xminusc = ((*ash_ptr_to_globals_misc).minusc );
	if (*xargv == ((void *)0)) {
		if (xminusc)
			ash_msg_and_raise_error(bb_msg_requires_arg, "-c");
		((*ash_ptr_to_globals_misc).optlist )[6] = 1;
	}
	if (((*ash_ptr_to_globals_misc).optlist )[3] == 2 && ((*ash_ptr_to_globals_misc).optlist )[6] == 1 && isatty(0) && isatty(1))
		((*ash_ptr_to_globals_misc).optlist )[3] = 1;
	if (((*ash_ptr_to_globals_misc).optlist )[4] == 2)
		((*ash_ptr_to_globals_misc).optlist )[4] = ((*ash_ptr_to_globals_misc).optlist )[3];
	for (i = 0; i < NOPTS; i++)
		if (((*ash_ptr_to_globals_misc).optlist )[i] == 2)
			((*ash_ptr_to_globals_misc).optlist )[i] = 0;

	

	/* POSIX 1003.2: first arg after -c cmd is $0, remainder $1... */
	if (xminusc) {
		((*ash_ptr_to_globals_misc).minusc ) = *xargv++;
		if (*xargv)
			goto setarg0;
	} else if (!((*ash_ptr_to_globals_misc).optlist )[6]) {
		setinputfile(*xargv, 0);
 setarg0:
		((*ash_ptr_to_globals_misc).arg0 ) = *xargv++;
		commandname = ((*ash_ptr_to_globals_misc).arg0 );
	}

	((*ash_ptr_to_globals_var).shellparam ).p = xargv;
#if definedEx(CONFIG_ASH_GETOPTS)
	((*ash_ptr_to_globals_var).shellparam ).optind = 1;
	((*ash_ptr_to_globals_var).shellparam ).optoff = -1;
#endif
	/* assert(shellparam.malloced == 0 && shellparam.nparam == 0); */
	while (*xargv) {
		((*ash_ptr_to_globals_var).shellparam ).nparam++;
		xargv++;
	}
	optschanged();
}

/*
 * Read /etc/profile or .profile.
 */
static void
read_profile(const char *name)
{
	int skip;

	if (setinputfile(name, INPUT_PUSH_FILE | INPUT_NOFILE_OK) < 0)
		return;
	skip = cmdloop(0);
	popfile();
	if (skip)
		exitshell();
}

/*
 * This routine is called when an error or an interrupt occurs in an
 * interactive shell and control is returned to the main command loop.
 */
static void
reset(void)
{
	/* from eval.c: */
	evalskip = 0;
	loopnest = 0;
	/* from input.c: */
	g_parsefile->left_in_buffer = 0;
	g_parsefile->left_in_line = 0;      /* clear input buffer */
	popallfiles();
	/* from parser.c: */
	tokpushback = 0;
	checkkwd = 0;
	/* from redir.c: */
	clearredir(/*drop:*/ 0);
}





