			if (!docd(p, flags))
				goto out;
			break;
		}
	} while (path);
	ash_msg_and_raise_error("can't cd to %s", dest);
	/* NOTREACHED */
 out:
	if (flags & 2)
		out1fmt("%s\n", ((*ash_ptr_to_globals_misc).curdir ));
	return 0;
}

static int 
pwdcmd(int argc __attribute__ ((__unused__)), char **argv __attribute__ ((__unused__)))
{
	int flags;
