static int need_to_remember(struct redirtab *rp, int fd)
{
	int i;

	if (!rp) /* remembering was not requested */
		return 0;

	for (i = 0; i < rp->pair_count; i++) {
		if (rp->two_fd[i].orig == fd) {
			/* already remembered */
			return 0;
		}
	}
	return 1;
}

/* "hidden" fd is a fd used to read scripts, or a copy of such */
static int is_hidden_fd(struct redirtab *rp, int fd)
{
	int i;
	struct parsefile *pf;

	if (fd == -1)
		return 0;
	/* Check open scripts' fds */
	pf = g_parsefile;
	while (pf) {
		/* We skip pf_fd == 0 case because of the following case:
		 * $ ash  # running ash interactively
		 * $ . ./script.sh
		 * and in script.sh: "exec 9>&0".
		 * Even though top-level pf_fd _is_ 0,
		 * it's still ok to use it: "read" builtin uses it,
		 * why should we cripple "exec" builtin?
		 */
		if (pf->pf_fd > 0 && fd == pf->pf_fd) {
			return 1;
		}
		pf = pf->prev;
	}

	if (!rp)
		return 0;
	/* Check saved fds of redirects */
	fd |= COPYFD_RESTORE;
	for (i = 0; i < rp->pair_count; i++) {
		if (rp->two_fd[i].copy == fd) {
			return 1;
		}
	}
	return 0;
}

/*
 * Process a list of redirection commands.  If the REDIR_PUSH flag is set,
 * old file descriptors are stashed away so that the redirection can be
 * undone by calling popredir.
 */
/* flags passed to redirect */


static void
redirect(union node *redir, int flags)
{
	struct redirtab *sv;
	int sv_pos;
	int i;
	int fd;
	int newfd;
	int copied_fd2 = -1;

	((*ash_ptr_to_globals_var).g_nullredirs )++;
	if (!redir) {
		return;
	}

	sv = ((void *)0);
	sv_pos = 0;
	do { ((*ash_ptr_to_globals_misc).suppress_int )++; do { __asm__ __volatile__ ("": : :"memory"); } while (0); } while (0);
	if (flags & 01) {
		union node *tmp = redir;
		do {
			sv_pos++;
#if definedEx(CONFIG_ASH_BASH_COMPAT)
			if (tmp->nfile.type == 17)
				sv_pos++;
#endif
			tmp = tmp->nfile.next;
		} while (tmp);
		sv = xmalloc(sizeof(*sv) + sv_pos * sizeof(sv->two_fd[0]));
		sv->next = ((*ash_ptr_to_globals_var).redirlist);
		sv->pair_count = sv_pos;
		((*ash_ptr_to_globals_var).redirlist) = sv;
		sv->nullredirs = ((*ash_ptr_to_globals_var).g_nullredirs ) - 1;
		((*ash_ptr_to_globals_var).g_nullredirs ) = 0;
		while (sv_pos > 0) {
			sv_pos--;
			sv->two_fd[sv_pos].orig = sv->two_fd[sv_pos].copy = -2;
		}
	}

	do {
		int right_fd = -1;
		fd = redir->nfile.fd;
		if (redir->nfile.type == 22 || redir->nfile.type == 23) {
			right_fd = redir->ndup.dupfd;
			//bb_error_msg("doing %d > %d", fd, right_fd);
			/* redirect from/to same file descriptor? */
			if (right_fd == fd)
				continue;
			/* "echo >&10" and 10 is a fd opened to a sh script? */
			if (is_hidden_fd(sv, right_fd)) {
				(*bb_errno) = 9; /* as if it is closed */
				ash_msg_and_raise_error("%d: %m", right_fd);
			}
			newfd = -1;
		} else {
			newfd = openredirect(redir); /* always >= 0 */
			if (fd == newfd) {
				/* Descriptor wasn't open before redirect.
				 * Mark it for close in the future */
				if (need_to_remember(sv, fd)) {
					goto remember_to_close;
				}
				continue;
			}
		}
#if definedEx(CONFIG_ASH_BASH_COMPAT)
 redirect_more:
#endif
		if (need_to_remember(sv, fd)) {
			/* Copy old descriptor */
			/* Careful to not accidentally "save"
			 * to the same fd as right side fd in N>&M */
			int minfd = right_fd < 10 ? 10 : right_fd + 1;
			i = fcntl(fd, 0, minfd);
/* You'd expect copy to be CLOEXECed. Currently these extra "saved" fds
 * are closed in popredir() in the child, preventing them from leaking
 * into child. (popredir() also cleans up the mess in case of failures)
 */
			if (i == -1) {
				i = (*bb_errno);
				if (i != 9) {
					/* Strange error (e.g. "too many files" EMFILE?) */
					if (newfd >= 0)
						close(newfd);
					(*bb_errno) = i;
					ash_msg_and_raise_error("%d: %m", fd);
					/* NOTREACHED */
				}
				/* EBADF: it is not open - good, remember to close it */
 remember_to_close:
				i = -3;
			} else { /* fd is open, save its copy */
				/* "exec fd>&-" should not close fds
				 * which point to script file(s).
				 * Force them to be restored afterwards */
				if (is_hidden_fd(sv, fd))
					i |= COPYFD_RESTORE;
			}
			if (fd == 2)
				copied_fd2 = i;
			sv->two_fd[sv_pos].orig = fd;
			sv->two_fd[sv_pos].copy = i;
			sv_pos++;
		}
		if (newfd < 0) {
			/* NTOFD/NFROMFD: copy redir->ndup.dupfd to fd */
			if (redir->ndup.dupfd < 0) { /* "fd>&-" */
				/* Don't want to trigger debugging */
				if (fd != -1)
					close(fd);
			} else {
				copyfd(redir->ndup.dupfd, fd | COPYFD_EXACT);
			}
		} else if (fd != newfd) { /* move newfd to fd */
			copyfd(newfd, fd | COPYFD_EXACT);
#if definedEx(CONFIG_ASH_BASH_COMPAT)
			if (!(redir->nfile.type == 17 && fd == 2))
#endif
				close(newfd);
		}
#if definedEx(CONFIG_ASH_BASH_COMPAT)
		if (redir->nfile.type == 17 && fd == 1) {
			/* We already redirected it to fd 1, now copy it to 2 */
			newfd = 1;
			fd = 2;
			goto redirect_more;
		}
#endif
	} while ((redir = redir->nfile.next) != ((void *)0));

	int_on();
	if ((flags & 03) && copied_fd2 >= 0)
		((*ash_ptr_to_globals_var).preverrout_fd) = copied_fd2;
}

/*
 * Undo the effects of the last redirection.
 */
static void
popredir(int drop, int restore)
{
	struct redirtab *rp;
	int i;

	if (--((*ash_ptr_to_globals_var).g_nullredirs ) >= 0)
		return;
	do { ((*ash_ptr_to_globals_misc).suppress_int )++; do { __asm__ __volatile__ ("": : :"memory"); } while (0); } while (0);
	rp = ((*ash_ptr_to_globals_var).redirlist);
	for (i = 0; i < rp->pair_count; i++) {
		int fd = rp->two_fd[i].orig;
		int copy = rp->two_fd[i].copy;
		if (copy == -3) {
			if (!drop)
				close(fd);
			continue;
		}
		if (copy != -2) {
			if (!drop || (restore && (copy & COPYFD_RESTORE))) {
				copy &= ~COPYFD_RESTORE;
				/*close(fd);*/
				copyfd(copy, fd | COPYFD_EXACT);
			}
			close(copy & ~COPYFD_RESTORE);
		}
	}
	((*ash_ptr_to_globals_var).redirlist) = rp->next;
	((*ash_ptr_to_globals_var).g_nullredirs ) = rp->nullredirs;
	free(rp);
	int_on();
}

/*
 * Undo all redirections.  Called on error or interrupt.
 */

/*
 * Discard all saved file descriptors.
 */
static void
clearredir(int drop)
{
	for (;;) {
		((*ash_ptr_to_globals_var).g_nullredirs ) = 0;
		if (!((*ash_ptr_to_globals_var).redirlist))
			break;
		popredir(drop, /*restore:*/ 0);
	}
}

static int
redirectsafe(union node *redir, int flags)
{
	int err;
	volatile int saveint;
	struct jmploc *volatile savehandler = ((*ash_ptr_to_globals_misc).exception_handler);
	struct jmploc jmploc;

	((saveint) = ((*ash_ptr_to_globals_misc).suppress_int ));
	/* "echo 9>/dev/null; echo >&9; echo result: $?" - result should be 1, not 2! */
	err = _setjmp (jmploc.loc); // huh?? was = setjmp(jmploc.loc) * 2;
	if (!err) {
		((*ash_ptr_to_globals_misc).exception_handler) = &jmploc;
		redirect(redir, flags);
	}
	((*ash_ptr_to_globals_misc).exception_handler) = savehandler;
	if (err && ((*ash_ptr_to_globals_misc).exception_type ) != 1)
		longjmp(((*ash_ptr_to_globals_misc).exception_handler)->loc, 1);
	do { do { __asm__ __volatile__ ("": : :"memory"); } while (0); ((*ash_ptr_to_globals_misc).suppress_int ) = (saveint); if (((*ash_ptr_to_globals_misc).suppress_int ) == 0 && ((*ash_ptr_to_globals_misc).pending_int )) raise_interrupt(); } while (0);
	return err;
}


