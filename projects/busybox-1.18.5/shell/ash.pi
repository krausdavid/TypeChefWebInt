 * know that the current directory has changed.
 */
static int
docd(const char *dest, int flags)
{
	const char *dir = ((void *)0);
	int err;

	;

	do { ((*ash_ptr_to_globals_misc).suppress_int )++; do { __asm__ __volatile__ ("": : :"memory"); } while (0); } while (0);
