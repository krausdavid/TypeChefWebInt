static int 
cdcmd(int argc __attribute__ ((__unused__)), char **argv __attribute__ ((__unused__)))
{
	const char *dest;
	const char *path;
	const char *p;
	char c;
	struct stat statb;
	int flags;

	flags = cdopt();
	dest = *argptr;
	if (!dest)
		dest = bltinlookup("HOME");
	else if (((dest)[0] == '-' && !(dest)[1])) {
		dest = bltinlookup("OLDPWD");
		flags |= 2;
	}
	if (!dest)
		dest = ((*ash_ptr_to_globals_misc).nullstr );
	if (*dest == '/')
		goto step7;
	if (*dest == '.') {
		c = dest[1];
 dotdot:
		switch (c) {
		case '\0':
		case '/':
			goto step6;
		case '.':
			c = dest[2];
			if (c != '.')
				goto dotdot;
		}
	}
	if (!*dest)
		dest = ".";
	path = bltinlookup("CDPATH");
	if (!path) {
 step6:
 step7:
		p = dest;
		goto docd;
	}
	do {
		c = *path;
		p = path_advance(&path, dest);
		if (stat(p, &statb) >= 0 && ((((statb.st_mode)) & 0170000) == (0040000))) {
			if (c && c != ':')
				flags |= 2;
