	} else
		tokpushback = 1;
	n1 = parse_command();
	if (readtoken() == 8) {
		pipenode = stzalloc(sizeof(struct npipe));
		pipenode->type = 1;
		/*pipenode->npipe.pipe_backgnd = 0; - stzalloc did it */
		lp = stzalloc(sizeof(struct nodelist));
		pipenode->npipe.cmdlist = lp;
		lp->n = n1;
		do {
			prev = lp;
			lp = stzalloc(sizeof(struct nodelist));
			checkkwd = 0x4 | 0x2 | 0x1;
			lp->n = parse_command();
			prev->next = lp;
