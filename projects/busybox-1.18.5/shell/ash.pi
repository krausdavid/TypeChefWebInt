				goto out;
			}
		}
	}
 out:
	popstackmark(&smark);
}

/*
 * Kick off a subshell to evaluate a tree.
 */
static void
evalsubshell(union node *n, int flags)
{
	struct job *jp;
	int backgnd = (n->type == 3);
	int status;

	expredir(n->nredir.redirect);
	if (!backgnd && (flags & 01) && !((*ash_ptr_to_globals_misc).may_have_traps ))
		goto nofork;
	do { ((*ash_ptr_to_globals_misc).suppress_int )++; do { __asm__ __volatile__ ("": : :"memory"); } while (0); } while (0);
	jp = makejob(/*n,*/ 1);
	if (forkshell(jp, n, backgnd) == 0) {
		/* child */
		int_on();
		flags |= 01;
		if (backgnd)
			flags &= ~02;
 nofork:
		redirect(n->nredir.redirect, 0);
		evaltreenr(n->nredir.n, flags);
		/* never returns */
	}
	status = 0;
	if (!backgnd)
		status = waitforjob(jp);
	exitstatus = status;
	int_on();
