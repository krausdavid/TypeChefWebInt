#if definedEx(CONFIG_ASH_GETOPTS)
static int
getopts(char *optstr, char *optvar, char **optfirst, int *param_optind, int *optoff)
{
	char *p, *q;
	char c = '?';
	int done = 0;
	int err = 0;
	char s[12];
	char **optnext;

	if (*param_optind < 1)
		return 1;
	optnext = optfirst + *param_optind - 1;

	if (*param_optind <= 1 || *optoff < 0 || (int)strlen(optnext[-1]) < *optoff)
		p = ((void *)0);
	else
		p = optnext[-1] + *optoff;
	if (p == ((void *)0) || *p == '\0') {
		/* Current word is done, advance */
		p = *optnext;
		if (p == ((void *)0) || *p != '-' || *++p == '\0') {
 atend:
			p = ((void *)0);
			done = 1;
			goto out;
		}
		optnext++;
		if (((p)[0] == '-' && !(p)[1]))        /* check for "--" */
			goto atend;
	}

	c = *p++;
	for (q = optstr; *q != c;) {
		if (*q == '\0') {
			if (optstr[0] == ':') {
				s[0] = c;
				s[1] = '\0';
				err |= setvarsafe("OPTARG", s, 0);
			} else {
				fprintf(stderr, "Illegal option -%c\n", c);
				unsetvar("OPTARG");
			}
			c = '?';
			goto out;
		}
		if (*++q == ':')
			q++;
	}

	if (*++q == ':') {
		if (*p == '\0' && (p = *optnext) == ((void *)0)) {
			if (optstr[0] == ':') {
				s[0] = c;
				s[1] = '\0';
				err |= setvarsafe("OPTARG", s, 0);
				c = ':';
			} else {
				fprintf(stderr, "No arg for -%c option\n", c);
				unsetvar("OPTARG");
				c = '?';
			}
			goto out;
		}

		if (p == *optnext)
			optnext++;
		err |= setvarsafe("OPTARG", p, 0);
		p = ((void *)0);
	} else
		err |= setvarsafe("OPTARG", ((*ash_ptr_to_globals_misc).nullstr ), 0);
 out:
	*optoff = p ? p - *(optnext - 1) : -1;
	*param_optind = optnext - optfirst + 1;
	fmtstr(s, sizeof(s), "%d", *param_optind);
	err |= setvarsafe("OPTIND", s, 0x40);
	s[0] = c;
	s[1] = '\0';
	err |= setvarsafe(optvar, s, 0);
	if (err) {
		*param_optind = 1;
		*optoff = -1;
		flush_stdout_stderr();
		raise_exception(1);
	}
	return done;
}

/*
 * The getopts builtin.  Shellparam.optnext points to the next argument
 * to be processed.  Shellparam.optptr points to the next character to
 * be processed in the current argument.  If shellparam.optnext is NULL,
 * then it's the first time getopts has been called.
 */
static int 
getoptscmd(int argc, char **argv)
{
	char **optbase;

	if (argc < 3)
		ash_msg_and_raise_error("usage: getopts optstring var [arg]");
	if (argc == 3) {
		optbase = ((*ash_ptr_to_globals_var).shellparam ).p;
		if (((*ash_ptr_to_globals_var).shellparam ).optind > ((*ash_ptr_to_globals_var).shellparam ).nparam + 1) {
			((*ash_ptr_to_globals_var).shellparam ).optind = 1;
			((*ash_ptr_to_globals_var).shellparam ).optoff = -1;
		}
	} else {
		optbase = &argv[3];
		if (((*ash_ptr_to_globals_var).shellparam ).optind > argc - 2) {
			((*ash_ptr_to_globals_var).shellparam ).optind = 1;
			((*ash_ptr_to_globals_var).shellparam ).optoff = -1;
		}
	}

	return getopts(argv[1], argv[2], optbase, &((*ash_ptr_to_globals_var).shellparam ).optind,
			&((*ash_ptr_to_globals_var).shellparam ).optoff);
}
