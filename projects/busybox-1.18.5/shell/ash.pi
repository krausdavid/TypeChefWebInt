			updatetbl = 0;
			cmdp = ((void *)0);
		} else if (cmdp->rehash == 0)
			/* if not invalidated by cd, we're done */
			goto success;
	}

	/* If %builtin not in path, check for builtin next */
	bcmd = find_builtin(name);
	if (bcmd) {
		if (((bcmd)->name[0] & 2))
			goto builtin_success;
		if (act & 0x08) {
			if (!(act & 0x20))
				goto builtin_success;
		} else if (builtinloc <= 0) {
			goto builtin_success;
