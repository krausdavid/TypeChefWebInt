/* ============ The cd and pwd commands */




static int
cdopt(void)
{
	int flags = 0;
	int i, j;

	j = 'L';
	while ((i = nextopt("LP")) != '\0') {
		if (i != j) {
			flags ^= 1;
			j = i;
		}
	}

	return flags;
}

/*
 * Update curdir (the name of the current directory) in response to a
 * cd command.
 */
static const char *
updatepwd(const char *dir)
{
	char *new;
	char *p;
	char *cdcomppath;
	const char *lim;

	cdcomppath = ststrdup(dir);
	((new) = ((void *)((*ash_ptr_to_globals_memstack).g_stacknxt )));
	if (*dir != '/') {
		if (((*ash_ptr_to_globals_misc).curdir ) == ((*ash_ptr_to_globals_misc).nullstr ))
			return 0;
		new = stack_putstr(((*ash_ptr_to_globals_misc).curdir ), new);
	}
	new = makestrspace(strlen(dir) + 2, new);
	lim = (char *)((void *)((*ash_ptr_to_globals_memstack).g_stacknxt )) + 1;
	if (*dir != '/') {
		if (new[-1] != '/')
			(*(new)++ = ('/'));
		if (new > lim && *lim == '/')
			lim++;
	} else {
		(*(new)++ = ('/'));
		cdcomppath++;
		if (dir[1] == '/' && dir[2] != '/') {
			(*(new)++ = ('/'));
			cdcomppath++;
			lim++;
		}
	}
	p = strtok(cdcomppath, "/");
	while (p) {
		switch (*p) {
		case '.':
			if (p[1] == '.' && p[2] == '\0') {
				while (new > lim) {
					(--(new));
					if (new[-1] == '/')
						break;
				}
				break;
			}
			if (p[1] == '\0')
				break;
			/* fall through */
		default:
			new = stack_putstr(p, new);
			(*(new)++ = ('/'));
		}
		p = strtok(0, "/");
	}
	if (new > lim)
		(--(new));
	*new = 0;
	return ((void *)((*ash_ptr_to_globals_memstack).g_stacknxt ));
}

