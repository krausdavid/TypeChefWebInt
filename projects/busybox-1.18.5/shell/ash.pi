argstr(char *p, int flags, struct strlist *var_str_list)
{
	static const char spclchars[] __attribute__((aligned(1))) = {
		'=',
		':',
		((unsigned char)'\210'),
		((unsigned char)'\203'),
		((unsigned char)'\201'),
		((unsigned char)'\202'),
		((unsigned char)'\204'),
		((unsigned char)'\204') | 01,
#if definedEx(CONFIG_SH_MATH_SUPPORT)
		((unsigned char)'\207'),
#endif
		'\0'
	};
	const char *reject = spclchars;
	int quotes = flags & (0x1 | 0x10 | 0x8); /* do CTLESC */
	int breakall = flags & 0x80;
	int inquotes;
	size_t length;
	int startloc;

	if (!(flags & 0x4)) {
		reject += 2;
	} else if (flags & 0x40) {
		reject++;
	}
	inquotes = 0;
	length = 0;
	if (flags & 0x2) {
		char *q;

		flags &= ~0x2;
 tilde:
		q = p;
		if ((unsigned char)*q == ((unsigned char)'\201') && (flags & 0x100))
			q++;
		if (*q == '~')
			p = exptilde(p, q, flags);
	}
 start:
	startloc = expdest - (char *)((void *)((*ash_ptr_to_globals_memstack).g_stacknxt ));
	for (;;) {
		unsigned char c;

		length += strcspn(p + length, reject);
		c = p[length];
		if (c) {
			if (!(c & 0x80)
			
#if definedEx(CONFIG_SH_MATH_SUPPORT)
|| c == ((unsigned char)'\207')
#endif
#if !definedEx(CONFIG_SH_MATH_SUPPORT)

#endif

			) {
				/* c == '=' || c == ':' || c == CTLENDARI */
				length++;
			}
		}
		if (length > 0) {
			int newloc;
			expdest = stack_nputstr(p, length, expdest);
			newloc = expdest - (char *)((void *)((*ash_ptr_to_globals_memstack).g_stacknxt ));
			if (breakall && !inquotes && newloc > startloc) {
				recordregion(startloc, newloc, 0);
			}
			startloc = newloc;
		}
		p += length + 1;
		length = 0;

		switch (c) {
		case '\0':
			goto breakloop;
		case '=':
			if (flags & 0x40) {
				p--;
				continue;
			}
			flags |= 0x40;
			reject++;
			/* fall through */
		case ':':
			/*
			 * sort of a hack - expand tildes in variable
			 * assignments (after the first '=' and after ':'s).
			 */
			if (*--p == '~') {
				goto tilde;
			}
			continue;
		}

		switch (c) {
		case ((unsigned char)'\203'): /* ??? */
			goto breakloop;
		case ((unsigned char)'\210'):
			/* "$@" syntax adherence hack */
			if (!inquotes
			 && memcmp(p, dolatstr, 4) == 0
			 && (  p[4] == (char)((unsigned char)'\210')
			    || (p[4] == (char)((unsigned char)'\203') && p[5] == (char)((unsigned char)'\210'))
			    )
			) {
				p = evalvar(p + 1, flags, /* var_str_list: */ ((void *)0)) + 1;
				goto start;
			}
			inquotes = !inquotes;
 addquote:
			if (quotes) {
				p--;
				length++;
				startloc++;
			}
			break;
		case ((unsigned char)'\201'):
			startloc++;
			length++;
			goto addquote;
		case ((unsigned char)'\202'):
			p = evalvar(p, flags, var_str_list);
			goto start;
		case ((unsigned char)'\204'):
			c = '\0';
		case ((unsigned char)'\204')|01:
			expbackq(argbackq->n, c, quotes);
			argbackq = argbackq->next;
			goto start;
#if definedEx(CONFIG_SH_MATH_SUPPORT)
		case ((unsigned char)'\207'):
			p--;
			expari(quotes);
			goto start;
#endif
		}
	}
 breakloop: ;
}

static char *
scanleft(char *startp, char *rmesc, char *rmescend __attribute__ ((__unused__)),
		char *pattern, int quotes, int zero)
{
	char *loc, *loc2;
	char c;

	loc = startp;
	loc2 = rmesc;
	do {
		int match;
		const char *s = loc2;

		c = *loc2;
		if (zero) {
			*loc2 = '\0';
			s = rmesc;
		}
		match = !fnmatch((pattern), (s), 0);

		*loc2 = c;
		if (match)
			return loc;
		if (quotes && (unsigned char)*loc == ((unsigned char)'\201'))
			loc++;
		loc++;
		loc2++;
	} while (c);
	return ((void *)0);
}

static char *
scanright(char *startp, char *rmesc, char *rmescend,
		char *pattern, int quotes, int match_at_start)
{
#if !definedEx(CONFIG_ASH_OPTIMIZE_FOR_SIZE)
	int try2optimize = match_at_start;
#endif
	int esc = 0;
	char *loc;
	char *loc2;

	/* If we called by "${v/pattern/repl}" or "${v//pattern/repl}":
	 * startp="escaped_value_of_v" rmesc="raw_value_of_v"
	 * rmescend=""(ptr to NUL in rmesc) pattern="pattern" quotes=match_at_start=1
	 * Logic:
	 * loc starts at NUL at the end of startp, loc2 starts at the end of rmesc,
	 * and on each iteration they go back two/one char until they reach the beginning.
	 * We try to find a match in "raw_value_of_v", "raw_value_of_", "raw_value_of" etc.
	 */
	/* TODO: document in what other circumstances we are called. */

	for (loc = pattern - 1, loc2 = rmescend; loc >= startp; loc2--) {
		int match;
		char c = *loc2;
		const char *s = loc2;
		if (match_at_start) {
			*loc2 = '\0';
			s = rmesc;
		}
		match = !fnmatch((pattern), (s), 0);
		//bb_error_msg("pmatch(pattern:'%s',s:'%s'):%d", pattern, s, match);
		*loc2 = c;
		if (match)
			return loc;
#if !definedEx(CONFIG_ASH_OPTIMIZE_FOR_SIZE)
		if (try2optimize) {
			/* Maybe we can optimize this:
			 * if pattern ends with unescaped *, we can avoid checking
			 * shorter strings: if "foo*" doesnt match "raw_value_of_v",
			 * it wont match truncated "raw_value_of_" strings too.
			 */
			unsigned plen = strlen(pattern);
			/* Does it end with "*"? */
			if (plen != 0 && pattern[--plen] == '*') {
				/* "xxxx*" is not escaped */
				/* "xxx\*" is escaped */
				/* "xx\\*" is not escaped */
				/* "x\\\*" is escaped */
				int slashes = 0;
				while (plen != 0 && pattern[--plen] == '\\')
					slashes++;
				if (!(slashes & 1))
					break; /* ends with unescaped "*" */
			}
			try2optimize = 0;
		}
#endif
		loc--;
		if (quotes) {
			if (--esc < 0) {
				esc = esclen(startp, loc);
			}
			if (esc % 2) {
				esc--;
				loc--;
			}
		}
	}
	return ((void *)0);
}

static void varunset(const char *, const char *, const char *, int) __attribute__ ((__noreturn__));
static void
varunset(const char *end, const char *var, const char *umsg, int varflags)
{
	const char *msg;
	const char *tail;

	tail = ((*ash_ptr_to_globals_misc).nullstr );
	msg = "parameter not set";
	if (umsg) {
		if ((unsigned char)*end == ((unsigned char)'\203')) {
			if (varflags & 0x10)
				tail = " or null";
		} else {
			msg = umsg;
		}
	}
	ash_msg_and_raise_error("%.*s: %s%s", (int)(end - var - 1), var, msg, tail);
}

#if definedEx(CONFIG_ASH_BASH_COMPAT)
static char *
parse_sub_pattern(char *arg, int varflags)
{
	char *idx, *repl = ((void *)0);
	unsigned char c;

	//char *org_arg = arg;
	//bb_error_msg("arg:'%s' varflags:%x", arg, varflags);
	idx = arg;
	while (1) {
		c = *arg;
		if (!c)
			break;
		if (c == '/') {
			/* Only the first '/' seen is our separator */
			if (!repl) {
				repl = idx + 1;
				c = '\0';
			}
		}
		*idx++ = c;
		arg++;
		/*
		 * Example: v='ab\c'; echo ${v/\\b/_\\_\z_}
		 * The result is a_\_z_c (not a\_\_z_c)!
		 *
		 * Enable debug prints in this function and you'll see:
		 * ash: arg:'\\b/_\\_z_' varflags:d
		 * ash: pattern:'\\b' repl:'_\_z_'
		 * That is, \\b is interpreted as \\b, but \\_ as \_!
		 * IOW: search pattern and replace string treat backslashes
		 * differently! That is the reason why we check repl below:
		 */
		if (c == '\\' && *arg == '\\' && repl && !(varflags & 0x80))
			arg++; /* skip both '\', not just first one */
	}
	*idx = c; /* NUL */
	//bb_error_msg("pattern:'%s' repl:'%s'", org_arg, repl);

	return repl;
}
