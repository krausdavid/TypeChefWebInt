static void
grabstackblock(size_t len)
{
	len = (((len) + SHELL_SIZE) & ~SHELL_SIZE);
	((*ash_ptr_to_globals_memstack).g_stacknxt ) += len;
	((*ash_ptr_to_globals_memstack).g_stacknleft) -= len;
}

/*
 * The following routines are somewhat easier to use than the above.
 * The user declares a variable of type STACKSTR, which may be declared
 * to be a register.  The macro STARTSTACKSTR initializes things.  Then
 * the user uses the macro STPUTC to add characters to the string.  In
 * effect, STPUTC(c, p) is the same as *p++ = c except that the stack is
 * grown as necessary.  When the user is done, she can just leave the
 * string there and refer to it using stackblock().  Or she can allocate
 * the space for it using grabstackstr().  If it is necessary to allow
 * someone else to use the stack temporarily and then continue to grow
 * the string, the user should use grabstack to allocate the space, and
 * then call ungrabstr(p) to return to the previous mode of operation.
 *
 * USTPUTC is like STPUTC except that it doesn't check for overflow.
 * CHECKSTACKSPACE can be called before USTPUTC to ensure that there
 * is space for at least one character.
 */
static void *
growstackstr(void)
{
	size_t len = ((*ash_ptr_to_globals_memstack).g_stacknleft);
	if (((*ash_ptr_to_globals_memstack).herefd ) >= 0 && len >= 1024) {
		full_write(((*ash_ptr_to_globals_memstack).herefd ), ((void *)((*ash_ptr_to_globals_memstack).g_stacknxt )), len);
		return ((void *)((*ash_ptr_to_globals_memstack).g_stacknxt ));
	}
	growstackblock();
	return (char *)((void *)((*ash_ptr_to_globals_memstack).g_stacknxt )) + len;
}

/*
 * Called from CHECKSTRSPACE.
 */
static char *
makestrspace(size_t newlen, char *p)
{
	size_t len = p - ((*ash_ptr_to_globals_memstack).g_stacknxt );
	size_t size = ((*ash_ptr_to_globals_memstack).g_stacknleft);

	for (;;) {
		size_t nleft;

		size = ((*ash_ptr_to_globals_memstack).g_stacknleft);
		nleft = size - len;
		if (nleft >= newlen)
			break;
		growstackblock();
	}
	return (char *)((void *)((*ash_ptr_to_globals_memstack).g_stacknxt )) + len;
}

static char *
stack_nputstr(const char *s, size_t n, char *p)
{
	p = makestrspace(n, p);
	p = (char *)memcpy(p, s, n) + n;
	return p;
}

static char *
stack_putstr(const char *s, char *p)
{
	return stack_nputstr(s, strlen(s), p);
}

static char *
_STPUTC(int c, char *p)
{
	if (p == ((*ash_ptr_to_globals_memstack).sstrend ))
		p = growstackstr();
	*p++ = c;
	return p;
}

























/* ============ String helpers */

/*
 * prefix -- see if pfx is a prefix of string.
 */
static char *
prefix(const char *string, const char *pfx)
{
	while (*pfx) {
		if (*pfx++ != *string++)
			return ((void *)0);
	}
	return (char *) string;
}

/*
 * Check for a valid number.  This should be elsewhere.
 */
static int
is_number(const char *p)
{
	do {
		if (!((unsigned char)((*p) - '0') <= 9))
			return 0;
	} while (*++p != '\0');
	return 1;
}

/*
 * Convert a string of digits to an integer, printing an error message on
 * failure.
 */
static int
number(const char *s)
{
	if (!is_number(s))
		ash_msg_and_raise_error("Illegal number: %s", s);
	return atoi(s);
}

/*
 * Produce a possibly single quoted string suitable as input to the shell.
 * The return string is allocated on the stack.
 */
static char *
single_quote(const char *s)
{
	char *p;

	((p) = ((void *)((*ash_ptr_to_globals_memstack).g_stacknxt )));

	do {
		char *q;
		size_t len;

		len = strchrnul(s, '\'') - s;

		q = p = makestrspace(len + 3, p);

		*q++ = '\'';
		q = (char *)memcpy(q, s, len) + len;
		*q++ = '\'';
		s += len;

		((p) += (q - p));

		if (*s != '\'')
			break;
		len = 0;
		do len++; while (*++s == '\'');

		q = p = makestrspace(len + 3, p);

		*q++ = '"';
		q = (char *)memcpy(q, s - len, len) + len;
		*q++ = '"';

		((p) += (q - p));
	} while (*s);

	(*(p)++ = ('\0'));

	return ((void *)((*ash_ptr_to_globals_memstack).g_stacknxt ));
}


/* ============ nextopt */

static char **argptr;                  /* argument list for builtin commands */
static char *optionarg;                /* set by nextopt (like getopt) */
static char *optptr;                   /* used by nextopt */

/*
 * XXX - should get rid of. Have all builtins use getopt(3).
 * The library getopt must have the BSD extension static variable
 * "optreset", otherwise it can't be used within the shell safely.
 *
 * Standard option processing (a la getopt) for builtin routines.
 * The only argument that is passed to nextopt is the option string;
 * the other arguments are unnecessary. It returns the character,
 * or '\0' on end of input.
 */
static int
nextopt(const char *optstring)
{
	char *p;
	const char *q;
	char c;

	p = optptr;
	if (p == ((void *)0) || *p == '\0') {
		/* We ate entire "-param", take next one */
		p = *argptr;
		if (p == ((void *)0))
			return '\0';
		if (*p != '-')
			return '\0';
		if (*++p == '\0') /* just "-" ? */
			return '\0';
		argptr++;
		if (((p)[0] == '-' && !(p)[1])) /* "--" ? */
			return '\0';
		/* p => next "-param" */
	}
	/* p => some option char in the middle of a "-param" */
	c = *p++;
	for (q = optstring; *q != c;) {
		if (*q == '\0')
			ash_msg_and_raise_error("illegal option -%c", c);
		if (*++q == ':')
			q++;
	}
	if (*++q == ':') {
		if (*p == '\0') {
			p = *argptr++;
			if (p == ((void *)0))
				ash_msg_and_raise_error("no arg for -%c option", c);
		}
		optionarg = p;
		p = ((void *)0);
	}
	optptr = p;
	return c;
}


