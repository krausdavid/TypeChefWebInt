			sep = 1 << 8;
			goto param;
		}
		/* fall through */
	case '*':
		sep = ((((*ash_ptr_to_globals_var).varinit )[0].flags & 0x20) == 0) ? (unsigned char)((((*ash_ptr_to_globals_var).varinit )[0].var_text + 4)[0]) : ' ';
		i = 
#if !definedEx(CONFIG_ASH_OPTIMIZE_FOR_SIZE)
((S_I_T[syntax_index_table[sep]] >> ((syntax)*4)) & 0xf)
#endif
#if definedEx(CONFIG_ASH_OPTIMIZE_FOR_SIZE)
SIT(sep, syntax)
#endif
;
