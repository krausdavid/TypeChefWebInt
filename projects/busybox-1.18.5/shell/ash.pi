/*
 * Read and execute commands.
 * "Top" is nonzero for the top level command loop;
 * it turns on prompting if the shell is interactive.
 */
static int
cmdloop(int top)
{
	union node *n;
	struct stackmark smark;
	int inter;
	int numeof = 0;

	;
	for (;;) {
		int skip;

		setstackmark(&smark);
#if definedEx(CONFIG_ASH_JOB_CONTROL)
		if (doing_jobctl)
			showjobs(stderr, 0x04);
#endif
		inter = 0;
		if (((*ash_ptr_to_globals_misc).optlist )[3] && top) {
			inter++;
#if definedEx(CONFIG_ASH_MAIL)
			chkmail();
#endif
		}
		n = parsecmd(inter);

		
			

		if (n == ((union node *) -1L)) {
			if (!top || numeof >= 50)
				break;
			if (!stoppedjobs()) {
				if (!((*ash_ptr_to_globals_misc).optlist )[2])
					break;
				out2str("\nUse \"exit\" to leave shell.\n");
			}
			numeof++;
		} else if (((*ash_ptr_to_globals_misc).optlist )[5] == 0) {
			/* job_warning can only be 2,1,0. Here 2->1, 1/0->0 */
			((*ash_ptr_to_globals_misc).job_warning) >>= 1;
			numeof = 0;
			evaltree(n, 0);
		}
		popstackmark(&smark);
		skip = evalskip;

		if (skip) {
			evalskip = 0;
			return skip & (1 << 4);
		}
	}
	return 0;
}

/*
 * Take commands from a file.  To be compatible we should do a path
 * search for the file, which is necessary to find sub-commands.
 */
static char *
find_dot_file(char *name)
{
	char *fullname;
	const char *path = (
#if definedEx(CONFIG_ASH_MAIL)
(&(&(&((*ash_ptr_to_globals_var).varinit )[0])[1])[1])[1]
#endif
#if !definedEx(CONFIG_ASH_MAIL)
(&((*ash_ptr_to_globals_var).varinit )[0])[1]
#endif
.var_text + 5);
	struct stat statb;

	/* don't try this for absolute or relative paths */
	if (strchr(name, '/'))
		return name;

	/* IIRC standards do not say whether . is to be searched.
	 * And it is even smaller this way, making it unconditional for now:
	 */
	if (1) { /* ENABLE_ASH_BASH_COMPAT */
		fullname = name;
		goto try_cur_dir;
	}

	while ((fullname = path_advance(&path, name)) != ((void *)0)) {
 try_cur_dir:
		if ((stat(fullname, &statb) == 0) && ((((statb.st_mode)) & 0170000) == (0100000))) {
			/*
			 * Don't bother freeing here, since it will
			 * be freed by the caller.
			 */
			return fullname;
		}
		if (fullname != name)
			stunalloc(fullname);
	}

	/* not found in the PATH */
	ash_msg_and_raise_error("%s: not found", name);
	/* NOTREACHED */
}

static int 
dotcmd(int argc, char **argv)
{
	char *fullname;
	struct strlist *sp;
	volatile struct shparam saveparam;

	for (sp = cmdenviron; sp; sp = sp->next)
		setvareq(xstrdup(sp->text), 0x04 | 0x08);

	if (!argv[1]) {
		/* bash says: "bash: .: filename argument required" */
		return 2; /* bash compat */
	}

	/* "false; . empty_file; echo $?" should print 0, not 1: */
	exitstatus = 0;

	fullname = find_dot_file(argv[1]);

	argv += 2;
	argc -= 2;
	if (argc) { /* argc > 0, argv[0] != NULL */
		saveparam = ((*ash_ptr_to_globals_var).shellparam );
		((*ash_ptr_to_globals_var).shellparam ).malloced = 0;
		((*ash_ptr_to_globals_var).shellparam ).nparam = argc;
		((*ash_ptr_to_globals_var).shellparam ).p = argv;
	};

	setinputfile(fullname, INPUT_PUSH_FILE);
	commandname = fullname;
	cmdloop(0);
	popfile();

	if (argc) {
		freeparam(&((*ash_ptr_to_globals_var).shellparam ));
		((*ash_ptr_to_globals_var).shellparam ) = saveparam;
	};

	return exitstatus;
}

static int 
exitcmd(int argc __attribute__ ((__unused__)), char **argv)
{
	if (stoppedjobs())
		return 0;
	if (argv[1])
		exitstatus = number(argv[1]);
	raise_exception(4);
	/* NOTREACHED */
}

