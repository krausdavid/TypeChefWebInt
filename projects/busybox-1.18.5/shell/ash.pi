/*
 * Returns true if the text contains nothing to expand (no dollar signs
 * or backquotes).
 */
static int
noexpand(const char *text)
{
	unsigned char c;

	while ((c = *text++) != '\0') {
		if (c == ((unsigned char)'\210'))
			continue;
		if (c == ((unsigned char)'\201'))
			text++;
		else if (
#if !definedEx(CONFIG_ASH_OPTIMIZE_FOR_SIZE)
((S_I_T[syntax_index_table[c]] >> ((0)*4)) & 0xf)
#endif
#if definedEx(CONFIG_ASH_OPTIMIZE_FOR_SIZE)
SIT(c, 0)
#endif
 == 12)
			return 0;
	}
	return 1;
}

static void
parsefname(void)
{
	union node *n = redirnode;

	if (readtoken() != 3)
		raise_error_unexpected_syntax(-1);
	if (n->type == 24) {
		struct heredoc *here = heredoc;
		struct heredoc *p;
		int i;

		if (quoteflag == 0)
			n->type = 25;
		;
		if (!noexpand(wordtext) || (i = strlen(wordtext)) == 0 || i > 79)
			raise_error_syntax("illegal eof marker for << redirection");
		rmescapes(wordtext, 0);
		here->eofmark = wordtext;
		here->next = ((void *)0);
		if (heredoclist == ((void *)0))
			heredoclist = here;
		else {
			for (p = heredoclist; p->next; p = p->next)
				continue;
			p->next = here;
		}
	} else if (n->type == 22 || n->type == 23) {
		fixredir(n, wordtext, 0);
	} else {
		n->nfile.fname = makename();
	}
}

static union node *
simplecmd(void)
{
	union node *args, **app;
	union node *n = ((void *)0);
	union node *vars, **vpp;
	union node **rpp, *redir;
	int savecheckkwd;
#if definedEx(CONFIG_ASH_BASH_COMPAT)
	smallint double_brackets_flag = 0;
#endif
	args = ((void *)0);
	app = &args;
	vars = ((void *)0);
	vpp = &vars;
	redir = ((void *)0);
	rpp = &redir;

	savecheckkwd = 0x1;
	for (;;) {
		int t;
		checkkwd = savecheckkwd;
		t = readtoken();
		switch (t) {
#if definedEx(CONFIG_ASH_BASH_COMPAT)
		case 6: /* "&&" */
		case 7: /* "||" */
			if (!double_brackets_flag) {
				tokpushback = 1;
				goto out;
			}
			wordtext = (char *) (t == 6 ? "-a" : "-o");
#endif
		case 3:
			n = stzalloc(sizeof(struct narg));
			n->type = 15;
			/*n->narg.next = NULL; - stzalloc did it */
			n->narg.text = wordtext;
#if definedEx(CONFIG_ASH_BASH_COMPAT)
			if (strcmp("[[", wordtext) == 0)
				double_brackets_flag = 1;
			else if (strcmp("]]", wordtext) == 0)
				double_brackets_flag = 0;
#endif
			n->narg.backquote = backquotelist;
			if (savecheckkwd && isassignment(wordtext)) {
				*vpp = n;
				vpp = &n->narg.next;
			} else {
				*app = n;
				app = &n->narg.next;
				savecheckkwd = 0;
			}
			break;
		case 2:
			*rpp = n = redirnode;
			rpp = &n->nfile.next;
			parsefname();   /* read name of redirection file */
			break;
		case 9:
			if (args && app == &args->narg.next
			 && !vars && !redir
			) {
				struct builtincmd *bcmd;
				const char *name;

				/* We have a function */
				if (readtoken() != 10)
					raise_error_unexpected_syntax(10);
				name = n->narg.text;
				if (!goodname(name)
				 || ((bcmd = find_builtin(name)) && ((bcmd)->name[0] & 1))
				) {
					raise_error_syntax("bad function name");
				}
				n->type = 14;
				checkkwd = 0x4 | 0x2 | 0x1;
				n->narg.next = parse_command();
				return n;
			}
			/* fall through */
		default:
			tokpushback = 1;
			goto out;
		}
	}
 out:
	*app = ((void *)0);
	*vpp = ((void *)0);
	*rpp = ((void *)0);
	n = stzalloc(sizeof(struct ncmd));
	n->type = 0;
	n->ncmd.args = args;
	n->ncmd.assign = vars;
	n->ncmd.redirect = redir;
	return n;
}

