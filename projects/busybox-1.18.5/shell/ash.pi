	case '$':
		num = ((*ash_ptr_to_globals_misc).rootpid );
		goto numvar;
	case '?':
		num = exitstatus;
		goto numvar;
	case '#':
		num = ((*ash_ptr_to_globals_var).shellparam ).nparam;
		goto numvar;
