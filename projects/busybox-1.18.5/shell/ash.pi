/*
 * Ignore a signal.
 */
static void
ignoresig(int signo)
{
	/* Avoid unnecessary system calls. Is it already SIG_IGNed? */
	if (((*ash_ptr_to_globals_misc).sigmode )[signo - 1] != 3 && ((*ash_ptr_to_globals_misc).sigmode )[signo - 1] != 4) {
		/* No, need to do it */
		signal(signo, ((__sighandler_t) 1));
	}
	((*ash_ptr_to_globals_misc).sigmode )[signo - 1] = 4;
}

/*
 * Only one usage site - in setsignal()
 */
static void
signal_handler(int signo)
{
	((*ash_ptr_to_globals_misc).gotsig )[signo - 1] = 1;

	if (signo == 2 && !((*ash_ptr_to_globals_misc).trap )[2]) {
		if (!((*ash_ptr_to_globals_misc).suppress_int )) {
			((*ash_ptr_to_globals_misc).pending_sig ) = 0;
			raise_interrupt(); /* does not return */
		}
		((*ash_ptr_to_globals_misc).pending_int ) = 1;
	} else {
		((*ash_ptr_to_globals_misc).pending_sig ) = signo;
	}
}

/*
 * Set the signal handler for the specified signal.  The routine figures
 * out what it should be set to.
 */
static void
setsignal(int signo)
{
	char *t;
	char cur_act, new_act;
	struct sigaction act;

	t = ((*ash_ptr_to_globals_misc).trap )[signo];
	new_act = 1;
	if (t != ((void *)0)) { /* trap for this sig is set */
		new_act = 2;
		if (t[0] == '\0') /* trap is "": ignore this sig */
			new_act = 3;
	}

	if ((!((*ash_ptr_to_globals_misc).shlvl )) && new_act == 1) {
		switch (signo) {
		case 2:
			if (((*ash_ptr_to_globals_misc).optlist )[3] || ((*ash_ptr_to_globals_misc).minusc ) || ((*ash_ptr_to_globals_misc).optlist )[6] == 0)
				new_act = 2;
			break;
		case 3:

			
				

			/* man bash:
			 * "In all cases, bash ignores SIGQUIT. Non-builtin
			 * commands run by bash have signal handlers
			 * set to the values inherited by the shell
			 * from its parent". */
			new_act = 3;
			break;
		case 15:
			if (((*ash_ptr_to_globals_misc).optlist )[3])
				new_act = 3;
			break;
#if definedEx(CONFIG_ASH_JOB_CONTROL)
		case 20:
		case 22:
			if (((*ash_ptr_to_globals_misc).optlist )[4])
				new_act = 3;
			break;
#endif
		}
	}
//TODO: if !rootshell, we reset SIGQUIT to DFL,
//whereas we have to restore it to what shell got on entry
//from the parent. See comment above

	t = &((*ash_ptr_to_globals_misc).sigmode )[signo - 1];
	cur_act = *t;
	if (cur_act == 0) {
		/* current setting is not yet known */
		if (sigaction(signo, ((void *)0), &act)) {
			/* pretend it worked; maybe we should give a warning,
			 * but other shells don't. We don't alter sigmode,
			 * so we retry every time.
			 * btw, in Linux it never fails. --vda */
			return;
		}
		if (act.__sigaction_handler.sa_handler == ((__sighandler_t) 1)) {
			cur_act = 4;
			if (((*ash_ptr_to_globals_misc).optlist )[4]
			 && (signo == 20 || signo == 21 || signo == 22)
			) {
				cur_act = 3;   /* don't hard ignore these */
			}
		}
	}
	if (cur_act == 4 || cur_act == new_act)
		return;

	act.__sigaction_handler.sa_handler = ((__sighandler_t) 0);
	switch (new_act) {
	case 2:
		act.__sigaction_handler.sa_handler = signal_handler;
		act.sa_flags = 0; /* matters only if !DFL and !IGN */
		sigfillset(&act.sa_mask); /* ditto */
		break;
	case 3:
		act.__sigaction_handler.sa_handler = ((__sighandler_t) 1);
		break;
	}
	sigaction_set(signo, &act);

	*t = new_act;
}

