/* Wrapper around strcmp for qsort/bsearch/... */
static int
pstrcmp(const void *a, const void *b)
{
	return strcmp((char*) a, (*(char**) b) + 1);
}

static const char *const *
findkwd(const char *s)
{
	return bsearch(s, tokname_array + 13,
			((unsigned)(sizeof(tokname_array) / sizeof((tokname_array)[0]))) - 13,
			sizeof(tokname_array[0]), pstrcmp);
}

/*
 * Locate and print what a word is...
 */
static int
describe_command(char *command, int describe_command_verbose)
{
	struct cmdentry entry;
	struct tblentry *cmdp;
#if definedEx(CONFIG_ASH_ALIAS)
	const struct alias *ap;
#endif
	const char *path = (
#if definedEx(CONFIG_ASH_MAIL)
(&(&(&((*ash_ptr_to_globals_var).varinit )[0])[1])[1])[1]
#endif
#if !definedEx(CONFIG_ASH_MAIL)
(&((*ash_ptr_to_globals_var).varinit )[0])[1]
#endif
.var_text + 5);

	if (describe_command_verbose) {
		out1str(command);
	}

	/* First look at the keywords */
	if (findkwd(command)) {
		out1str(describe_command_verbose ? " is a shell keyword" : command);
		goto out;
	}

#if definedEx(CONFIG_ASH_ALIAS)
	/* Then look at the aliases */
	ap = lookupalias(command, 0);
	if (ap != ((void *)0)) {
		if (!describe_command_verbose) {
			out1str("alias ");
			printalias(ap);
			return 0;
		}
		out1fmt(" is an alias for %s", ap->val);
		goto out;
	}
#endif
	/* Then check if it is a tracked alias */
	cmdp = cmdlookup(command, 0);
	if (cmdp != ((void *)0)) {
		entry.cmdtype = cmdp->cmdtype;
		entry.u = cmdp->param;
	} else {
		/* Finally use brute force */
		find_command(command, &entry, 0x02, path);
	}

	switch (entry.cmdtype) {
	case 0: {
		int j = entry.u.index;
		char *p;
		if (j < 0) {
			p = command;
		} else {
			do {
				p = path_advance(&path, command);
				stunalloc(p);
			} while (--j >= 0);
		}
		if (describe_command_verbose) {
			out1fmt(" is%s %s",
				(cmdp ? " a tracked alias for" : ((*ash_ptr_to_globals_misc).nullstr )), p
			);
		} else {
			out1str(p);
		}
		break;
	}

	case 1:
		if (describe_command_verbose) {
			out1str(" is a shell function");
		} else {
			out1str(command);
		}
		break;

	case 2:
		if (describe_command_verbose) {
			out1fmt(" is a %sshell builtin",
				((entry.u.cmd)->name[0] & 1) ?
					"special " : ((*ash_ptr_to_globals_misc).nullstr )
			);
		} else {
			out1str(command);
		}
		break;

	default:
		if (describe_command_verbose) {
			out1str(": not found\n");
		}
		return 127;
	}
 out:
	out1str("\n");
	return 0;
}

static int 
typecmd(int argc __attribute__ ((__unused__)), char **argv)
{
	int i = 1;
	int err = 0;
	int verbose = 1;

	/* type -p ... ? (we don't bother checking for 'p') */
	if (argv[1] && argv[1][0] == '-') {
		i++;
		verbose = 0;
	}
	while (argv[i]) {
		err |= describe_command(argv[i++], verbose);
	}
	return err;
}

