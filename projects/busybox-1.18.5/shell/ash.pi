/*
 * Fork off a subshell.  If we are doing job control, give the subshell its
 * own process group.  Jp is a job structure that the job is to be added to.
 * N is the command that will be evaluated by the child.  Both jp and n may
 * be NULL.  The mode parameter can be one of the following:
 *      FORK_FG - Fork off a foreground process.
 *      FORK_BG - Fork off a background process.
 *      FORK_NOJOB - Like FORK_FG, but don't give the process its own
 *                   process group even if job control is on.
 *
 * When job control is turned off, background processes have their standard
 * input redirected to /dev/null (except for the second and later processes
 * in a pipeline).
 *
 * Called with interrupts off.
 */
/*
 * Clear traps on a fork.
 */
static void
clear_traps(void)
{
	char **tp;

	for (tp = ((*ash_ptr_to_globals_misc).trap ); tp < &((*ash_ptr_to_globals_misc).trap )[65]; tp++) {
		if (*tp && **tp) {      /* trap not NULL or "" (SIG_IGN) */
			do { ((*ash_ptr_to_globals_misc).suppress_int )++; do { __asm__ __volatile__ ("": : :"memory"); } while (0); } while (0);
			if (((*ash_ptr_to_globals_misc).trap_ptr ) == ((*ash_ptr_to_globals_misc).trap ))
				free(*tp);
			/* else: it "belongs" to trap_ptr vector, don't free */
			*tp = ((void *)0);
			if ((tp - ((*ash_ptr_to_globals_misc).trap )) != 0)
				setsignal(tp - ((*ash_ptr_to_globals_misc).trap ));
			int_on();
		}
	}
	((*ash_ptr_to_globals_misc).may_have_traps ) = 0;
}

