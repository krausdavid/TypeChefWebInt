static void
fixredir(union node *n, const char *text, int err)
{
	int fd;

	;
	if (!err)
		n->ndup.vname = ((void *)0);

	fd = bb_strtou(text, ((void *)0), 10);
	if (!(*bb_errno) && fd >= 0)
		n->ndup.dupfd = fd;
	else if (((text)[0] == '-' && !(text)[1]))
		n->ndup.dupfd = -1;
