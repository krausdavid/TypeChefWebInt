		((*ash_ptr_to_globals_misc).trap_ptr ) = memcpy(xmalloc(sizeof(((*ash_ptr_to_globals_misc).trap ))), ((*ash_ptr_to_globals_misc).trap ), sizeof(((*ash_ptr_to_globals_misc).trap )));
