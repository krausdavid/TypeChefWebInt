/*
 * Same as setvar except that the variable and value are passed in
 * the first argument as name=value.  Since the first argument will
 * be actually stored in the table, it should not be a string that
 * will go away.
 * Called with interrupts off.
 */
static void
setvareq(char *s, int flags)
{
	struct var *vp, **vpp;

	vpp = hashvar(s);
	flags |= (0x01 & (((unsigned) (1 - ((*ash_ptr_to_globals_misc).optlist )[10])) - 1));
	vp = *findvar(vpp, s);
	if (vp) {
		if ((vp->flags & (0x02|
#if definedEx(CONFIG_ASH_RANDOM_SUPPORT)
0x200
#endif
#if !definedEx(CONFIG_ASH_RANDOM_SUPPORT)
0
#endif
)) == 0x02) {
			const char *n;

			if (flags & 0x100)
				free(s);
			n = vp->var_text;
			ash_msg_and_raise_error("%.*s: is read only", strchrnul(n, '=') - n, n);
		}

		if (flags & 0x80)
			return;

		if (vp->var_func && !(flags & 0x40))
			vp->var_func(var_end(s));

		if (!(vp->flags & (0x08|0x10)))
			free((char*)vp->var_text);

		flags |= vp->flags & ~(0x08|0x10|0x100|0x20);
	} else {
		/* variable s is not found */
		if (flags & 0x80)
			return;
		vp = xzalloc(sizeof(*vp));
		vp->next = *vpp;
		/*vp->func = NULL; - ckzalloc did it */
		*vpp = vp;
	}
	if (!(flags & (0x08|0x10|0x100)))
		s = xstrdup(s);
	vp->var_text = s;
	vp->flags = flags;
}

/*
 * Set the value of a variable.  The flags argument is ored with the
 * flags of the variable.  If val is NULL, the variable is unset.
 */
static void
setvar(const char *name, const char *val, int flags)
{
	const char *q;
	char *p;
	char *nameeq;
	size_t namelen;
	size_t vallen;

	q = endofname(name);
	p = strchrnul(q, '=');
	namelen = p - name;
	if (!namelen || p != q)
		ash_msg_and_raise_error("%.*s: bad variable name", namelen, name);
	vallen = 0;
	if (val == ((void *)0)) {
		flags |= 0x20;
	} else {
		vallen = strlen(val);
	}

	do { ((*ash_ptr_to_globals_misc).suppress_int )++; do { __asm__ __volatile__ ("": : :"memory"); } while (0); } while (0);
	nameeq = xmalloc(namelen + vallen + 2);
	p = memcpy(nameeq, name, namelen) + namelen;
	if (val) {
		*p++ = '=';
		p = memcpy(p, val, vallen) + vallen;
	}
	*p = '\0';
	setvareq(nameeq, flags | 0x100);
	int_on();
}

