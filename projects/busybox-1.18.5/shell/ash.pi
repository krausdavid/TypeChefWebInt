/*
 * Read a line from the script.
 */
static char *
pfgets(char *line, int len)
{
	char *p = line;
	int nleft = len;
	int c;

	while (--nleft > 0) {
		c = 
#if !definedEx(CONFIG_ASH_ALIAS)
pgetc()
#endif
#if definedEx(CONFIG_ASH_ALIAS)
pgetc_without_PEOA()
#endif
;
		if (c == 256) {
			if (p == line)
				return ((void *)0);
			break;
		}
		*p++ = c;
		if (c == '\n')
			break;
	}
	*p = '\0';
	return line;
