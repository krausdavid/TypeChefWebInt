static void
evalloop(union node *n, int flags)
{
	int status;

	loopnest++;
	status = 0;
	flags &= 02;
	for (;;) {
		int i;

		evaltree(n->nbinary.ch1, 02);
		if (evalskip) {
 skipping:
			if (evalskip == (1 << 1) && --skipcount <= 0) {
				evalskip = 0;
				continue;
			}
			if (evalskip == (1 << 0) && --skipcount <= 0)
				evalskip = 0;
			break;
		}
		i = exitstatus;
		if (n->type != 9)
			i = !i;
		if (i != 0)
			break;
		evaltree(n->nbinary.ch2, flags);
		status = exitstatus;
		if (evalskip)
			goto skipping;
