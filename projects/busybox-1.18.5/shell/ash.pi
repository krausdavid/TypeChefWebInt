static struct builtincmd *
find_builtin(const char *name)
{
	struct builtincmd *bp;

	bp = bsearch(
		name, builtintab, ((unsigned)(sizeof(builtintab) / sizeof((builtintab)[0]))), sizeof(builtintab[0]),
		pstrcmp
