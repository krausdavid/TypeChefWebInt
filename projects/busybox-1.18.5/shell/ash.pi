	cmdp = cmdlookup(name, 0);
	if (cmdp != ((void *)0)) {
		int bit;

		switch (cmdp->cmdtype) {
		default:

			

		case 0:
			bit = 0x08;
			break;
		case 1:
			bit = 0x04;
			break;
		case 2:
			bit = 0x20;
			break;
		}
