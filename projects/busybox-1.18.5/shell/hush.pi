		rcode = run_list(pi);
	}
	/* free_pipe_list has the side effect of clearing memory.
	 * In the long run that function can be merged with run_list,
	 * but doing that now would hobble the debugging effort. */
	free_pipe_list(pi);
	do {} while (0);
	return rcode;
}


/* Called a few times only (or even once if "sh -c") */
static void init_sigmasks(void)
{
	unsigned sig;
	unsigned mask;
	sigset_t old_blocked_set;

	if (!(*ptr_to_globals).inherited_set_is_saved) {
		sigprocmask(2, ((void *)0), &(*ptr_to_globals).blocked_set);
		(*ptr_to_globals).inherited_set = (*ptr_to_globals).blocked_set;
	}
	old_blocked_set = (*ptr_to_globals).blocked_set;

	mask = (1 << 3);
	if (
#if definedEx(CONFIG_HUSH_INTERACTIVE)
((*ptr_to_globals).interactive_fd)
#endif
#if !definedEx(CONFIG_HUSH_INTERACTIVE)
0
#endif
) {
		mask = (1 << 3) | SPECIAL_INTERACTIVE_SIGS;
		if (
#if definedEx(CONFIG_HUSH_JOB)
((*ptr_to_globals).saved_tty_pgrp)
#endif
#if !definedEx(CONFIG_HUSH_JOB)
0
#endif
) /* we have ctty, job control sigs work */
			mask |= SPECIAL_JOB_SIGS;
	}
	(*ptr_to_globals).non_DFL_mask = mask;

	sig = 0;
	while (mask) {
		if (mask & 1)
			sigaddset(&(*ptr_to_globals).blocked_set, sig);
		mask >>= 1;
		sig++;
	}
	sigdelset(&(*ptr_to_globals).blocked_set, 17);

	if (memcmp(&old_blocked_set, &(*ptr_to_globals).blocked_set, sizeof(old_blocked_set)) != 0)
		sigprocmask(2, &(*ptr_to_globals).blocked_set, ((void *)0));

	/* POSIX allows shell to re-enable SIGCHLD
	 * even if it was SIG_IGN on entry */

	
	
		
 	if (!(*ptr_to_globals).inherited_set_is_saved)
		signal(17, ((__sighandler_t) 0));

	(*ptr_to_globals).inherited_set_is_saved = 1;
}

#if definedEx(CONFIG_HUSH_JOB)
/* helper */
static void maybe_set_to_sigexit(int sig)
{
	void (*handler)(int);
	/* non_DFL_mask'ed signals are, well, masked,
	 * no need to set handler for them.
	 */
	if (!(((*ptr_to_globals).non_DFL_mask >> sig) & 1)) {
		handler = signal(sig, sigexit);
		if (handler == ((__sighandler_t) 1)) /* oops... restore back to IGN! */
			signal(sig, handler);
	}
}
/* Set handlers to restore tty pgrp and exit */
static void set_fatal_handlers(void)
{
	/* We _must_ restore tty pgrp on fatal signals */
	if (1) {
		maybe_set_to_sigexit(4 );
		maybe_set_to_sigexit(8 );
		maybe_set_to_sigexit(7 );
		maybe_set_to_sigexit(11);
		maybe_set_to_sigexit(5);
	} /* else: hush is perfect. what SEGV? */
	maybe_set_to_sigexit(6);
	/* bash 3.2 seems to handle these just like 'fatal' ones */
	maybe_set_to_sigexit(13);
	maybe_set_to_sigexit(14);
	/* if we are interactive, SIGHUP, SIGTERM and SIGINT are masked.
	 * if we aren't interactive... but in this case
	 * we never want to restore pgrp on exit, and this fn is not called */
	/*maybe_set_to_sigexit(SIGHUP );*/
	/*maybe_set_to_sigexit(SIGTERM);*/
	/*maybe_set_to_sigexit(SIGINT );*/
}
#endif
static int set_mode(int state, char mode, const char *o_opt)
{
	int idx;
	switch (mode) {
	case 'n':
		(*ptr_to_globals).n_mode = state;
		break;
	case 'x':
		
#if definedEx(CONFIG_HUSH_MODE_X)


((*ptr_to_globals).x_mode)
 

 = state;
#endif
#if !definedEx(CONFIG_HUSH_MODE_X)

#endif

		break;
	case 'o':
		if (!o_opt) {
			/* "set -+o" without parameter.
			 * in bash, set -o produces this output:
			 *  pipefail        off
			 * and set +o:
			 *  set +o pipefail
			 * We always use the second form.
			 */
			const char *p = o_opt_strings;
			idx = 0;
			while (*p) {
				printf("set %co %s\n", ((*ptr_to_globals).o_opt[idx] ? '-' : '+'), p);
				idx++;
				p += strlen(p) + 1;
			}
			break;
		}
		idx = index_in_strings(o_opt_strings, o_opt);
		if (idx >= 0) {
			(*ptr_to_globals).o_opt[idx] = state;
			break;
		}
	default:
		return 1;
	}
	return 0;
}

int hush_main(int argc, char **argv) 
#if definedEx(CONFIG_FEATURE_INDIVIDUAL)
__attribute__(( visibility("default") ))
#endif
#if !definedEx(CONFIG_FEATURE_INDIVIDUAL)

#endif
;
int hush_main(int argc, char **argv)
{
	int opt;
	unsigned builtin_argc;
	char **e;
	struct variable *cur_var;
	struct variable *shell_ver;

	do { do { (*(struct globals**)&ptr_to_globals) = (void*)(xzalloc(sizeof((*ptr_to_globals)))); __asm__ __volatile__("":::"memory"); } while (0); } while (0);
	if (0) /* if EXIT_SUCCESS == 0, it is already done */
		(*ptr_to_globals).last_exitcode = 0;
#if definedEx(CONFIG_NOMMU)
	(*ptr_to_globals).argv0_for_re_execing = argv[0];
#endif
	/* Deal with HUSH_VERSION */
	shell_ver = xzalloc(sizeof(*shell_ver));
	shell_ver->flg_export = 1;
	shell_ver->flg_read_only = 1;
	/* Code which handles ${var<op>...} needs writable values for all variables,
	 * therefore we xstrdup: */
	shell_ver->varstr = xstrdup(hush_version_str);
	/* Create shell local variables from the values
	 * currently living in the environment */
	do {} while (0);
	unsetenv("HUSH_VERSION"); /* in case it exists in initial env */
	(*ptr_to_globals).top_var = shell_ver;
	cur_var = (*ptr_to_globals).top_var;
	e = environ;
	if (e) while (*e) {
		char *value = strchr(*e, '=');
		if (value) { /* paranoia */
			cur_var->next = xzalloc(sizeof(*cur_var));
			cur_var = cur_var->next;
			cur_var->varstr = *e;
			cur_var->max_len = strlen(*e);
			cur_var->flg_export = 1;
		}
		e++;
	}
	/* (Re)insert HUSH_VERSION into env (AFTER we scanned the env!) */
	do {} while (0);
	putenv(shell_ver->varstr);

	/* Export PWD */
	set_pwd_var(/*exp:*/ 1);
	/* bash also exports SHLVL and _,
	 * and sets (but doesn't export) the following variables:
	 * BASH=/bin/bash
	 * BASH_VERSINFO=([0]="3" [1]="2" [2]="0" [3]="1" [4]="release" [5]="i386-pc-linux-gnu")
	 * BASH_VERSION='3.2.0(1)-release'
	 * HOSTTYPE=i386
	 * MACHTYPE=i386-pc-linux-gnu
	 * OSTYPE=linux-gnu
	 * HOSTNAME=<xxxxxxxxxx>
	 * PPID=<NNNNN> - we also do it elsewhere
	 * EUID=<NNNNN>
	 * UID=<NNNNN>
	 * GROUPS=()
	 * LINES=<NNN>
	 * COLUMNS=<NNN>
	 * BASH_ARGC=()
	 * BASH_ARGV=()
	 * BASH_LINENO=()
	 * BASH_SOURCE=()
	 * DIRSTACK=()
	 * PIPESTATUS=([0]="0")
	 * HISTFILE=/<xxx>/.bash_history
	 * HISTFILESIZE=500
	 * HISTSIZE=500
	 * MAILCHECK=60
	 * PATH=/usr/gnu/bin:/usr/local/bin:/bin:/usr/bin:.
	 * SHELL=/bin/bash
	 * SHELLOPTS=braceexpand:emacs:hashall:histexpand:history:interactive-comments:monitor
	 * TERM=dumb
	 * OPTERR=1
	 * OPTIND=1
	 * IFS=$' \t\n'
	 * PS1='\s-\v\$ '
	 * PS2='> '
	 * PS4='+ '
	 */

#if definedEx(CONFIG_HUSH_INTERACTIVE)
	(*ptr_to_globals).line_input_state = new_line_input_t(FOR_SHELL);
#if definedEx(CONFIG_HUSH_SAVEHISTORY)
	{
		const char *hp = get_local_var_value("HISTFILE");
		if (!hp) {
			hp = get_local_var_value("HOME");
			if (hp) {
				(*ptr_to_globals).line_input_state->hist_file = concat_path_file(hp, ".hush_history");
				//set_local_var(xasprintf("HISTFILE=%s", ...));
			}
		}
	}
#endif
#endif
	(*ptr_to_globals).global_argc = argc;
	(*ptr_to_globals).global_argv = argv;
	/* Initialize some more globals to non-zero values */
	
#if !definedEx(CONFIG_HUSH_INTERACTIVE)
((void)0)
#endif
#if definedEx(CONFIG_HUSH_INTERACTIVE)
cmdedit_update_prompt()
#endif
;

	if (_setjmp (die_jmp)) {
		/* xfunc has failed! die die die */
		/* no EXIT traps, this is an escape hatch! */
		(*ptr_to_globals).exiting = 1;
		hush_exit(xfunc_error_retval);
	}

	/* Shell is non-interactive at first. We need to call
	 * init_sigmasks() if we are going to execute "sh <script>",
	 * "sh -c <cmds>" or login shell's /etc/profile and friends.
	 * If we later decide that we are interactive, we run init_sigmasks()
	 * in order to intercept (more) signals.
	 */

	/* Parse options */
	/* http://www.opengroup.org/onlinepubs/9699919799/utilities/sh.html */
	builtin_argc = 0;
	while (1) {
		opt = getopt(argc, argv, "+c:xins"
#if definedEx(CONFIG_NOMMU)
				"<:$:R:V:"
#if definedEx(CONFIG_HUSH_FUNCTIONS)
				"F:"
#endif
#endif
		);
		if (opt <= 0)
			break;
		switch (opt) {
		case 'c':
			/* Possibilities:
			 * sh ... -c 'script'
			 * sh ... -c 'script' ARG0 [ARG1...]
			 * On NOMMU, if builtin_argc != 0,
			 * sh ... -c 'builtin' BARGV... "" ARG0 [ARG1...]
			 * "" needs to be replaced with NULL
			 * and BARGV vector fed to builtin function.
			 * Note: the form without ARG0 never happens:
			 * sh ... -c 'builtin' BARGV... ""
			 */
			if (!(*ptr_to_globals).root_pid) {
				(*ptr_to_globals).root_pid = getpid();
				(*ptr_to_globals).root_ppid = getppid();
			}
			(*ptr_to_globals).global_argv = argv + optind;
			(*ptr_to_globals).global_argc = argc - optind;
			if (builtin_argc) {
				/* -c 'builtin' [BARGV...] "" ARG0 [ARG1...] */
				const struct built_in_command *x;

				init_sigmasks();
				x = find_builtin(optarg);
				if (x) { /* paranoia */
					(*ptr_to_globals).global_argc -= builtin_argc; /* skip [BARGV...] "" */
					(*ptr_to_globals).global_argv += builtin_argc;
					(*ptr_to_globals).global_argv[-1] = ((void *)0); /* replace "" */
					(*ptr_to_globals).last_exitcode = x->b_function(argv + optind - 1);
				}
				goto final_return;
			}
			if (!(*ptr_to_globals).global_argv[0]) {
				/* -c 'script' (no params): prevent empty $0 */
				(*ptr_to_globals).global_argv--; /* points to argv[i] of 'script' */
				(*ptr_to_globals).global_argv[0] = argv[0];
				(*ptr_to_globals).global_argc++;
			} /* else -c 'script' ARG0 [ARG1...]: $0 is ARG0 */
			init_sigmasks();
			parse_and_run_string(optarg);
			goto final_return;
		case 'i':
			/* Well, we cannot just declare interactiveness,
			 * we have to have some stuff (ctty, etc) */
			/* G_interactive_fd++; */
			break;
		case 's':
			/* "-s" means "read from stdin", but this is how we always
			 * operate, so simply do nothing here. */
			break;
#if definedEx(CONFIG_NOMMU)
		case '<': /* "big heredoc" support */
			full_write1_str(optarg);
			_exit(0);
		case '$': {
			unsigned long long empty_trap_mask;

			(*ptr_to_globals).root_pid = bb_strtou(optarg, &optarg, 16);
			optarg++;
			(*ptr_to_globals).root_ppid = bb_strtou(optarg, &optarg, 16);
			optarg++;
			(*ptr_to_globals).last_bg_pid = bb_strtou(optarg, &optarg, 16);
			optarg++;
			(*ptr_to_globals).last_exitcode = bb_strtou(optarg, &optarg, 16);
			optarg++;
			builtin_argc = bb_strtou(optarg, &optarg, 16);
			optarg++;
			empty_trap_mask = bb_strtoull(optarg, &optarg, 16);
			if (empty_trap_mask != 0) {
				int sig;
				init_sigmasks();
				(*ptr_to_globals).traps = xzalloc(sizeof((*ptr_to_globals).traps[0]) * 65);
				for (sig = 1; sig < 65; sig++) {
					if (empty_trap_mask & (1LL << sig)) {
						(*ptr_to_globals).traps[sig] = xzalloc(1); /* == xstrdup(""); */
						sigaddset(&(*ptr_to_globals).blocked_set, sig);
					}
				}
				sigprocmask(2, &(*ptr_to_globals).blocked_set, ((void *)0));
			}
#if definedEx(CONFIG_HUSH_LOOPS)
			optarg++;
			(*ptr_to_globals).depth_of_loop = bb_strtou(optarg, &optarg, 16);
#endif
			break;
		}
		case 'R':
		case 'V':
			
#if ((definedEx(CONFIG_NOMMU) || definedEx(CONFIG_HUSH_LOCAL)) && !definedEx(CONFIG_HUSH_LOCAL) && (!definedEx(CONFIG_NOMMU) || !definedEx(CONFIG_HUSH_LOCAL)) && definedEx(CONFIG_NOMMU) && (definedEx(CONFIG_NOMMU) || !definedEx(CONFIG_HUSH_LOCAL)))
set_local_var(xstrdup(optarg), 0, opt == 'R')
#endif
#if ((!definedEx(CONFIG_NOMMU) && !definedEx(CONFIG_HUSH_LOCAL)) || definedEx(CONFIG_HUSH_LOCAL) || (!definedEx(CONFIG_NOMMU) && definedEx(CONFIG_HUSH_LOCAL)) || !definedEx(CONFIG_NOMMU) || (definedEx(CONFIG_NOMMU) && definedEx(CONFIG_HUSH_LOCAL)))
set_local_var(xstrdup(optarg), /*exp:*/ 0, /*lvl:*/ 0, /*ro:*/ opt == 'R')
#endif
;
			break;
#if definedEx(CONFIG_HUSH_FUNCTIONS)
		case 'F': {
			struct function *funcp = new_function(optarg);
			/* funcp->name is already set to optarg */
			/* funcp->body is set to NULL. It's a special case. */
			funcp->body_as_string = argv[optind];
			optind++;
			break;
		}
#endif
#endif
		case 'n':
		case 'x':
			if (set_mode(1, opt, ((void *)0)) == 0) /* no error */
				break;
		default:

			
					
			
 			bb_show_usage();

		}
	} /* option parsing loop */

	if (!(*ptr_to_globals).root_pid) {
		(*ptr_to_globals).root_pid = getpid();
		(*ptr_to_globals).root_ppid = getppid();
	}

	/* If we are login shell... */
	if (argv[0] && argv[0][0] == '-') {
		FILE *input;
		do {} while (0);
		input = fopen_for_read("/etc/profile");
		if (input != ((void *)0)) {
			close_on_exec_on(fileno(input));
			init_sigmasks();
			parse_and_run_file(input);
			fclose(input);
		}
		/* bash: after sourcing /etc/profile,
		 * tries to source (in the given order):
		 * ~/.bash_profile, ~/.bash_login, ~/.profile,
		 * stopping on first found. --noprofile turns this off.
		 * bash also sources ~/.bash_logout on exit.
		 * If called as sh, skips .bash_XXX files.
		 */
	}

	if (argv[optind]) {
		FILE *input;
		/*
		 * "bash <script>" (which is never interactive (unless -i?))
		 * sources $BASH_ENV here (without scanning $PATH).
		 * If called as sh, does the same but with $ENV.
		 */
		do {} while (0);
		(*ptr_to_globals).global_argv = argv + optind;
		(*ptr_to_globals).global_argc = argc - optind;
		input = xfopen_for_read(argv[optind]);
		close_on_exec_on(fileno(input));
		init_sigmasks();
		parse_and_run_file(input);
#if definedEx(CONFIG_FEATURE_CLEAN_UP)
		fclose(input);
#endif
		goto final_return;
	}

	/* Up to here, shell was non-interactive. Now it may become one.
	 * NB: don't forget to (re)run init_sigmasks() as needed.
	 */

	/* A shell is interactive if the '-i' flag was given,
	 * or if all of the following conditions are met:
	 *    no -c command
	 *    no arguments remaining or the -s flag given
	 *    standard input is a terminal
	 *    standard output is a terminal
	 * Refer to Posix.2, the description of the 'sh' utility.
	 */
#if definedEx(CONFIG_HUSH_JOB)
	if (isatty(0) && isatty(1)) {
		((*ptr_to_globals).saved_tty_pgrp) = tcgetpgrp(0);
		do {} while (0);
		if (((*ptr_to_globals).saved_tty_pgrp) < 0)
			((*ptr_to_globals).saved_tty_pgrp) = 0;

		/* try to dup stdin to high fd#, >= 255 */
		((*ptr_to_globals).interactive_fd) = fcntl(0, 0, 255);
		if (((*ptr_to_globals).interactive_fd) < 0) {
			/* try to dup to any fd */
			((*ptr_to_globals).interactive_fd) = dup(0);
			if (((*ptr_to_globals).interactive_fd) < 0) {
				/* give up */
				((*ptr_to_globals).interactive_fd) = 0;
				((*ptr_to_globals).saved_tty_pgrp) = 0;
			}
		}
// TODO: track & disallow any attempts of user
// to (inadvertently) close/redirect G_interactive_fd
	}
	do {} while (0);
	if (((*ptr_to_globals).interactive_fd)) {
		close_on_exec_on(((*ptr_to_globals).interactive_fd));

		if (((*ptr_to_globals).saved_tty_pgrp)) {
			/* If we were run as 'hush &', sleep until we are
			 * in the foreground (tty pgrp == our pgrp).
			 * If we get started under a job aware app (like bash),
			 * make sure we are now in charge so we don't fight over
			 * who gets the foreground */
			while (1) {
				pid_t shell_pgrp = getpgrp();
				((*ptr_to_globals).saved_tty_pgrp) = tcgetpgrp(((*ptr_to_globals).interactive_fd));
				if (((*ptr_to_globals).saved_tty_pgrp) == shell_pgrp)
					break;
				/* send TTIN to ourself (should stop us) */
				kill(- shell_pgrp, 21);
			}
		}

		/* Block some signals */
		init_sigmasks();

		if (((*ptr_to_globals).saved_tty_pgrp)) {
			/* Set other signals to restore saved_tty_pgrp */
			set_fatal_handlers();
			/* Put ourselves in our own process group
			 * (bash, too, does this only if ctty is available) */
			setpgrp(); /* is the same as setpgid(our_pid, our_pid); */
			/* Grab control of the terminal */
			tcsetpgrp(((*ptr_to_globals).interactive_fd), getpid());
		}
		/* -1 is special - makes xfuncs longjmp, not exit
		 * (we reset die_sleep = 0 whereever we [v]fork) */
		(die_sleep = -1); /* sets die_sleep = -1 */
	} else {
		init_sigmasks();
	}
#endif
#if (definedEx(CONFIG_HUSH_INTERACTIVE) && !definedEx(CONFIG_HUSH_JOB))
	/* No job control compiled in, only prompt/line editing */
	if (isatty(0) && isatty(1)) {
		((*ptr_to_globals).interactive_fd) = fcntl(0, 0, 255);
		if (((*ptr_to_globals).interactive_fd) < 0) {
			/* try to dup to any fd */
			((*ptr_to_globals).interactive_fd) = dup(0);
			if (((*ptr_to_globals).interactive_fd) < 0)
				/* give up */
				((*ptr_to_globals).interactive_fd) = 0;
		}
	}
	if (((*ptr_to_globals).interactive_fd)) {
		close_on_exec_on(((*ptr_to_globals).interactive_fd));
	}
	init_sigmasks();
#endif
#if (!definedEx(CONFIG_HUSH_INTERACTIVE) && !definedEx(CONFIG_HUSH_JOB))
	/* We have interactiveness code disabled */
	init_sigmasks();
#endif
	/* bash:
	 * if interactive but not a login shell, sources ~/.bashrc
	 * (--norc turns this off, --rcfile <file> overrides)
	 */

	if (!
#if definedEx(CONFIG_FEATURE_SH_EXTRA_QUIET)
1
#endif
#if !definedEx(CONFIG_FEATURE_SH_EXTRA_QUIET)
0
#endif
 && 
#if definedEx(CONFIG_HUSH_INTERACTIVE)
((*ptr_to_globals).interactive_fd)
#endif
#if !definedEx(CONFIG_HUSH_INTERACTIVE)
0
#endif
) {
		/* note: ash and hush share this string */
		printf("\n\n%s %s\n"
			
#if definedEx(CONFIG_HUSH_HELP)
"Enter 'help' for a list of built-in commands.\n"
#endif
#if !definedEx(CONFIG_HUSH_HELP)

#endif

			"\n",
			bb_banner,
			"hush - the humble shell"
		);
	}

	parse_and_run_file(stdin);

 final_return:
	hush_exit((*ptr_to_globals).last_exitcode);
}


