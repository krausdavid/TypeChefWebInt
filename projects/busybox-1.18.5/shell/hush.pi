static int  builtin_memleak(char **argv __attribute__ ((__unused__)))
{
	void *p;
	unsigned long l;


	/* Optional. Reduces probability of false positives */
	malloc_trim(0);

	/* Crude attempt to find where "free memory" starts,
	 * sans fragmentation. */
	p = malloc(240);
	l = (unsigned long)p;
	free(p);
	p = malloc(3400);
	if (l < (unsigned long)p) l = (unsigned long)p;
	free(p);

	if (!(*ptr_to_globals).memleak_value)
		(*ptr_to_globals).memleak_value = l;

	l -= (*ptr_to_globals).memleak_value;
	if ((long)l < 0)
		l = 0;
	l /= 1024;
	if (l > 127)
		l = 127;

	/* Exitcode is "how many kilobytes we leaked since 1st call" */
	return l;
}

