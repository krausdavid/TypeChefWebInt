#endif
#if !definedEx(CONFIG_HUSH_TICK)

#endif

			if (!(first_ch & 0x80)) { /* unquoted $VAR */
				do {} while (0);
				if (val && val[0]) {
					n = expand_on_ifs(output, n, val);
					val = ((void *)0);
				}
			} else { /* quoted $VAR, val will be appended below */
				do {} while (0);
			}
			break;

		} /* switch (char after <SPECIAL_VAR_SYMBOL>) */

		if (val && val[0]) {
			o_addQstr(output, val);
		}
		free(to_be_freed);

