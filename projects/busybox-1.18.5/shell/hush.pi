/* "...\"...`..`...." - do we need to handle "...$(..)..." too? */
static void add_till_double_quote(o_string *dest, struct in_str *input)
{
	while (1) {
		int ch = ((input)->get(input));
		if (ch == (-1)) {
			syntax_error_unterm_ch('"');
			/*xfunc_die(); - redundant */
		}
		if (ch == '"')
			return;
		if (ch == '\\') {  /* \x. Copy both chars. */
			o_addchr(dest, ch);
			ch = ((input)->get(input));
		}
		o_addchr(dest, ch);
		if (ch == '`') {
			add_till_backquote(dest, input, /*in_dquote:*/ 1);
			o_addchr(dest, ch);
			continue;
		}
