		/* Note 1: must wait until there are no more children */
		/* Note 2: must be interruptible */
		/* Examples:
		 * $ sleep 3 & sleep 6 & wait
		 * [1] 30934 sleep 3
		 * [2] 30935 sleep 6
		 * [1] Done                   sleep 3
		 * [2] Done                   sleep 6
		 * $ sleep 3 & sleep 6 & wait
		 * [1] 30936 sleep 3
		 * [2] 30937 sleep 6
		 * [1] Done                   sleep 3
		 * ^C <-- after ~4 sec from keyboard
		 * $
		 */
		sigaddset(&(*ptr_to_globals).blocked_set, 17);
		sigprocmask(2, &(*ptr_to_globals).blocked_set, ((void *)0));
		while (1) {
			checkjobs(((void *)0));
			if ((*bb_errno) == 10)
				break;
			/* Wait for SIGCHLD or any other signal of interest */
			/* sigtimedwait with infinite timeout: */
			sig = sigwaitinfo(&(*ptr_to_globals).blocked_set, ((void *)0));
			if (sig > 0) {
				sig = check_and_run_traps(sig);
				if (sig && sig != 17) { /* see note 2 */
					ret = 128 + sig;
					break;
				}
			}
		}
		sigdelset(&(*ptr_to_globals).blocked_set, 17);
		sigprocmask(2, &(*ptr_to_globals).blocked_set, ((void *)0));
		return ret;
	}

	/* This is probably buggy wrt interruptible-ness */
	while (*argv) {
		pid_t pid = bb_strtou(*argv, ((void *)0), 10);
		if ((*bb_errno)) {
			/* mimic bash message */
			bb_error_msg("wait: '%s': not a pid or valid job spec", *argv);
			return 1;
		}
		if (waitpid(pid, &status, 0) == pid) {
			if ((((signed char) ((((__extension__ (((union { __typeof(status) __in; int __i; }) { .__in = (status) }).__i))) & 0x7f) + 1) >> 1) > 0))
				ret = 128 + (((__extension__ (((union { __typeof(status) __in; int __i; }) { .__in = (status) }).__i))) & 0x7f);
			else if (((((__extension__(((union { __typeof(status) __in; int __i; }) { .__in =(status) }).__i))) & 0x7f) == 0))
				ret = ((((__extension__ (((union { __typeof(status) __in; int __i; }) { .__in = (status) }).__i))) & 0xff00) >> 8);
			else /* wtf? */
				ret = 1;
		} else {
			bb_perror_msg("wait %s", *argv);
			ret = 127;
		}
		argv++;
	}

	return ret;
}

#if (definedEx(CONFIG_HUSH_LOOPS) || definedEx(CONFIG_HUSH_FUNCTIONS))
static unsigned parse_numeric_argv1(char **argv, unsigned def, unsigned def_min)
{
	if (argv[1]) {
		def = bb_strtou(argv[1], ((void *)0), 10);
		if ((*bb_errno) || def < def_min || argv[2]) {
			bb_error_msg("%s: bad arguments", argv[0]);
			def = (2147483647 * 2U + 1U);
		}
	}
	return def;
}
#endif
#if definedEx(CONFIG_HUSH_LOOPS)
static int  builtin_break(char **argv)
{
	unsigned depth;
	if ((*ptr_to_globals).depth_of_loop == 0) {
		bb_error_msg("%s: only meaningful in a loop", argv[0]);
		return 0; /* bash compat */
	}
	(*ptr_to_globals).flag_break_continue++; /* BC_BREAK = 1 */

	(*ptr_to_globals).depth_break_continue = depth = parse_numeric_argv1(argv, 1, 1);
	if (depth == (2147483647 * 2U + 1U))
		(*ptr_to_globals).flag_break_continue = BC_BREAK;
	if ((*ptr_to_globals).depth_of_loop < depth)
		(*ptr_to_globals).depth_break_continue = (*ptr_to_globals).depth_of_loop;

	return 0;
}

static int  builtin_continue(char **argv)
{
	(*ptr_to_globals).flag_break_continue = 1; /* BC_CONTINUE = 2 = 1+1 */
	return builtin_break(argv);
}
#endif
#if definedEx(CONFIG_HUSH_FUNCTIONS)
static int  builtin_return(char **argv)
{
	int rc;

	if ((*ptr_to_globals).flag_return_in_progress != -1) {
		bb_error_msg("%s: not in a function or sourced script", argv[0]);
		return 1; /* bash compat */
	}

	(*ptr_to_globals).flag_return_in_progress = 1;

	/* bash:
	 * out of range: wraps around at 256, does not error out
	 * non-numeric param:
	 * f() { false; return qwe; }; f; echo $?
	 * bash: return: qwe: numeric argument required  <== we do this
	 * 255  <== we also do this
	 */
	rc = parse_numeric_argv1(argv, (*ptr_to_globals).last_exitcode, 0);
	return rc;
}
#endif
