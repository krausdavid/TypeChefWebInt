static int  builtin_trap(char **argv)
{
	int sig;
	char *new_cmd;

	if (!(*ptr_to_globals).traps)
		(*ptr_to_globals).traps = xzalloc(sizeof((*ptr_to_globals).traps[0]) * 65);

	argv++;
	if (!*argv) {
		int i;
		/* No args: print all trapped */
		for (i = 0; i < 65; ++i) {
			if ((*ptr_to_globals).traps[i]) {
				printf("trap -- ");
				print_escaped((*ptr_to_globals).traps[i]);
				/* note: bash adds "SIG", but only if invoked
				 * as "bash". If called as "sh", or if set -o posix,
				 * then it prints short signal names.
				 * We are printing short names: */
				printf(" %s\n", get_signame(i));
			}
		}
		/*fflush_all(); - done after each builtin anyway */
		return 0;
	}

	new_cmd = ((void *)0);
	/* If first arg is a number: reset all specified signals */
	sig = bb_strtou(*argv, ((void *)0), 10);
	if ((*bb_errno) == 0) {
		int ret;
 process_sig_list:
		ret = 0;
		while (*argv) {
			sig = get_signum(*argv++);
			if (sig < 0 || sig >= 65) {
				ret = 1;
				/* Mimic bash message exactly */
				bb_perror_msg("trap: %s: invalid signal specification", argv[-1]);
				continue;
			}

			free((*ptr_to_globals).traps[sig]);
			(*ptr_to_globals).traps[sig] = xstrdup(new_cmd);

			do {} while (0);

			/* There is no signal for 0 (EXIT) */
			if (sig == 0)
				continue;

			if (new_cmd) {
				sigaddset(&(*ptr_to_globals).blocked_set, sig);
			} else {
				/* There was a trap handler, we are removing it
				 * (if sig has non-DFL handling,
				 * we don't need to do anything) */
				if (sig < 32 && ((*ptr_to_globals).non_DFL_mask & (1 << sig)))
					continue;
				sigdelset(&(*ptr_to_globals).blocked_set, sig);
			}
		}
		sigprocmask(2, &(*ptr_to_globals).blocked_set, ((void *)0));
		return ret;
	}

	if (!argv[1]) { /* no second arg */
		bb_error_msg("trap: invalid arguments");
		return 1;
	}

	/* First arg is "-": reset all specified to default */
	/* First arg is "--": skip it, the rest is "handler SIGs..." */
	/* Everything else: set arg as signal handler
	 * (includes "" case, which ignores signal) */
	if (argv[0][0] == '-') {
		if (argv[0][1] == '\0') { /* "-" */
			/* new_cmd remains NULL: "reset these sigs" */
			goto reset_traps;
		}
		if (argv[0][1] == '-' && argv[0][2] == '\0') { /* "--" */
			argv++;
		}
		/* else: "-something", no special meaning */
	}
	new_cmd = *argv;
 reset_traps:
	argv++;
	goto process_sig_list;
}

