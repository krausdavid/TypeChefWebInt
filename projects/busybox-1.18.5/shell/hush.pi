		(*ptr_to_globals).PS1 = get_local_var_value("PS1");
		if ((*ptr_to_globals).PS1 == ((void *)0))
			(*ptr_to_globals).PS1 = "\\w \\$ ";
		(*ptr_to_globals).PS2 = get_local_var_value("PS2");
