/* Restores tty foreground process group, and exits. */
static void hush_exit(int exitcode) __attribute__ ((__noreturn__));
static void hush_exit(int exitcode)
{
	if ((*ptr_to_globals).exiting <= 0 && (*ptr_to_globals).traps && (*ptr_to_globals).traps[0] && (*ptr_to_globals).traps[0][0]) {
		/* Prevent recursion:
		 * trap "echo Hi; exit" EXIT; exit
		 */
		char *argv[3];
		/* argv[0] is unused */
		argv[1] = (*ptr_to_globals).traps[0];
		argv[2] = ((void *)0);
		(*ptr_to_globals).exiting = 1; /* prevent EXIT trap recursion */
		/* Note: G.traps[0] is not cleared!
		 * "trap" will still show it, if executed
		 * in the handler */
		builtin_eval(argv);
	}

#if definedEx(CONFIG_FEATURE_CLEAN_UP)
	{
		struct variable *cur_var;
		if ((*ptr_to_globals).cwd != bb_msg_unknown)
			free((char*)(*ptr_to_globals).cwd);
		cur_var = (*ptr_to_globals).top_var;
		while (cur_var) {
			struct variable *tmp = cur_var;
			if (!cur_var->max_len)
				free(cur_var->varstr);
			cur_var = cur_var->next;
			free(tmp);
		}
	}
#endif
#if definedEx(CONFIG_HUSH_JOB)
	fflush_all();
	sigexit(- (exitcode & 0xff));
#endif
#if !definedEx(CONFIG_HUSH_JOB)
	exit(exitcode);
#endif
}


static int check_and_run_traps(int sig)
{
	/* I want it in rodata, not in bss.
	 * gcc 4.2.1 puts it in rodata only if it has { 0, 0 }
	 * initializer. But other compilers may still use bss.
	 * TODO: find more portable solution.
	 */
	static const struct timespec zero_timespec = { 0, 0 };
	smalluint save_rcode;
	int last_sig = 0;

	if (sig)
		goto jump_in;
	while (1) {
		sig = sigtimedwait(&(*ptr_to_globals).blocked_set, ((void *)0), &zero_timespec);
		if (sig <= 0)
			break;
 jump_in:
		last_sig = sig;
		if ((*ptr_to_globals).traps && (*ptr_to_globals).traps[sig]) {
			if ((*ptr_to_globals).traps[sig][0]) {
				/* We have user-defined handler */
				char *argv[3];
				/* argv[0] is unused */
				argv[1] = (*ptr_to_globals).traps[sig];
				argv[2] = ((void *)0);
				save_rcode = (*ptr_to_globals).last_exitcode;
				builtin_eval(argv);
				(*ptr_to_globals).last_exitcode = save_rcode;
			} /* else: "" trap, ignoring signal */
			continue;
		}
		/* not a trap: special action */
		switch (sig) {

		
			

			

		case 2:
			/* Builtin was ^C'ed, make it look prettier: */
			bb_putchar('\n');
			(*ptr_to_globals).flag_SIGINT = 1;
			break;
#if definedEx(CONFIG_HUSH_JOB)
		case 1: {
			struct pipe *job;
			/* bash is observed to signal whole process groups,
			 * not individual processes */
			for (job = (*ptr_to_globals).job_list; job; job = job->next) {
				if (job->pgrp <= 0)
					continue;
				do {} while (0);
				if (kill(- job->pgrp, 1) == 0)
					kill(- job->pgrp, 18);
			}
			sigexit(1);
		}
#endif
		default: /* ignored: */
			/* SIGTERM, SIGQUIT, SIGTTIN, SIGTTOU, SIGTSTP */
			break;
		}
	}
	return last_sig;
}


static const char *get_cwd(int force)
{
	if (force || (*ptr_to_globals).cwd == ((void *)0)) {
		/* xrealloc_getcwd_or_warn(arg) calls free(arg),
		 * we must not try to free(bb_msg_unknown) */
		if ((*ptr_to_globals).cwd == bb_msg_unknown)
			(*ptr_to_globals).cwd = ((void *)0);
		(*ptr_to_globals).cwd = xrealloc_getcwd_or_warn((char *)(*ptr_to_globals).cwd);
		if (!(*ptr_to_globals).cwd)
			(*ptr_to_globals).cwd = bb_msg_unknown;
	}
	return (*ptr_to_globals).cwd;
}


/*
 * Shell and environment variable support
 */
static struct variable **get_ptr_to_local_var(const char *name, unsigned len)
{
	struct variable **pp;
	struct variable *cur;

	pp = &(*ptr_to_globals).top_var;
	while ((cur = *pp) != ((void *)0)) {
		if (strncmp(cur->varstr, name, len) == 0 && cur->varstr[len] == '=')
			return pp;
		pp = &cur->next;
	}
	return ((void *)0);
}

static const char*  get_local_var_value(const char *name)
{
	struct variable **vpp;
	unsigned len = strlen(name);

	if ((*ptr_to_globals).expanded_assignments) {
		char **cpp = (*ptr_to_globals).expanded_assignments;
		while (*cpp) {
			char *cp = *cpp;
			if (strncmp(cp, name, len) == 0 && cp[len] == '=')
				return cp + len + 1;
			cpp++;
		}
	}

	vpp = get_ptr_to_local_var(name, len);
	if (vpp)
		return (*vpp)->varstr + len + 1;

	if (strcmp(name, "PPID") == 0)
		return utoa((*ptr_to_globals).root_ppid);
	// bash compat: UID? EUID?
#if definedEx(CONFIG_HUSH_RANDOM_SUPPORT)
	if (strcmp(name, "RANDOM") == 0)
		return utoa(next_random(&(*ptr_to_globals).random_gen));
#endif
	return ((void *)0);
}

