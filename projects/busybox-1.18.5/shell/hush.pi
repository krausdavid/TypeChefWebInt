			do {} while (0);
			res = expand_and_evaluate_arith(arg, ((void *)0));
			do {} while (0);
			sprintf(arith_buf, 
#if definedEx(CONFIG_SH_MATH_SUPPORT_64)
"%lld"
#endif
#if !definedEx(CONFIG_SH_MATH_SUPPORT_64)
"%ld"
#endif
