static void add_till_single_quote(o_string *dest, struct in_str *input)
{
	while (1) {
		int ch = ((input)->get(input));
		if (ch == (-1)) {
			syntax_error_unterm_ch('\'');
			/*xfunc_die(); - redundant */
		}
