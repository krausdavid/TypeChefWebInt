static const char *setup_prompt_string(int promptmode)
{
	const char *prompt_str;
	do {} while (0);
	if (!
#if (definedEx(CONFIG_FEATURE_EDITING_FANCY_PROMPT) && definedEx(CONFIG_HUSH_INTERACTIVE))
1
#endif
#if (!definedEx(CONFIG_HUSH_INTERACTIVE) || !definedEx(CONFIG_FEATURE_EDITING_FANCY_PROMPT))
0
#endif
) {
		/* Set up the prompt */
		if (promptmode == 0) { /* PS1 */
			free((char*)(*ptr_to_globals).PS1);
			/* bash uses $PWD value, even if it is set by user.
			 * It uses current dir only if PWD is unset.
			 * We always use current dir. */
			(*ptr_to_globals).PS1 = xasprintf("%s %c ", get_cwd(0), (geteuid() != 0) ? '$' : '#');
			prompt_str = (*ptr_to_globals).PS1;
		} else
			prompt_str = (*ptr_to_globals).PS2;
	} else
		prompt_str = (promptmode == 0) ? (*ptr_to_globals).PS1 : (*ptr_to_globals).PS2;
	do {} while (0);
	return prompt_str;
}

static void get_user_input(struct in_str *i)
{
	int r;
	const char *prompt_str;

	prompt_str = setup_prompt_string(i->promptmode);

	/* Enable command line editing only while a command line
	 * is actually being read */
	do {
		(*ptr_to_globals).flag_SIGINT = 0;
		/* buglet: SIGINT will not make new prompt to appear _at once_,
		 * only after <Enter>. (^C will work) */
		r = read_line_input(prompt_str, (*ptr_to_globals).user_input_buf, 1024-1, (*ptr_to_globals).line_input_state);
		/* catch *SIGINT* etc (^C is handled by read_line_input) */
		check_and_run_traps(0);
	} while (r == 0 || (*ptr_to_globals).flag_SIGINT); /* repeat if ^C or SIGINT */
	i->eof_flag = (r < 0);
	if (i->eof_flag) { /* EOF/error detected */
		(*ptr_to_globals).user_input_buf[0] = (-1); /* yes, it will be truncated, it's ok */
		(*ptr_to_globals).user_input_buf[1] = '\0';
	}
 	
		
		
		
		
		

	
	

	i->p = (*ptr_to_globals).user_input_buf;
}

#endif
/* This is the magic location that prints prompts
 * and gets data back from the user */
static int  file_get(struct in_str *i)
{
	int ch;

	/* If there is data waiting, eat it up */
	if (i->p && *i->p) {
#if definedEx(CONFIG_HUSH_INTERACTIVE)
 take_cached:
#endif
		ch = *i->p++;
		if (i->eof_flag && !*i->p)
			ch = (-1);
		/* note: ch is never NUL */
	} else {
		/* need to double check i->file because we might be doing something
		 * more complicated by now, like sourcing or substituting. */
#if definedEx(CONFIG_HUSH_INTERACTIVE)
		if (((*ptr_to_globals).interactive_fd) && i->promptme && i->file == stdin) {
			do {
				get_user_input(i);
			} while (!*i->p); /* need non-empty line */
			i->promptmode = 1; /* PS2 */
			i->promptme = 0;
			goto take_cached;
		}
#endif
		do ch = fgetc(i->file); while (ch == '\0');
	}
	do {} while (0);
#if definedEx(CONFIG_HUSH_INTERACTIVE)
	if (ch == '\n')
		i->promptme = 1;
#endif
	return ch;
}

/* All callers guarantee this routine will never
 * be used right after a newline, so prompting is not needed.
 */
static int  file_peek(struct in_str *i)
{
	int ch;
	if (i->p && *i->p) {
		if (i->eof_flag && !i->p[1])
			return (-1);
		return *i->p;
		/* note: ch is never NUL */
	}
	do ch = fgetc(i->file); while (ch == '\0');
	i->eof_flag = (ch == (-1));
	i->peek_buf[0] = ch;
	i->peek_buf[1] = '\0';
	i->p = i->peek_buf;
	do {} while (0);
	return ch;
}

static void setup_file_in_str(struct in_str *i, FILE *f)
{
	i->peek = file_peek;
	i->get = file_get;
#if definedEx(CONFIG_HUSH_INTERACTIVE)
	i->promptme = 1;
	i->promptmode = 0; /* PS1 */
#endif
	i->file = f;
	i->p = ((void *)0);
}

static void setup_string_in_str(struct in_str *i, const char *s)
{
	i->peek = static_peek;
	i->get = static_get;
#if definedEx(CONFIG_HUSH_INTERACTIVE)
	i->promptme = 1;
	i->promptmode = 0; /* PS1 */
#endif
	i->p = s;
	i->eof_flag = 0;
}


